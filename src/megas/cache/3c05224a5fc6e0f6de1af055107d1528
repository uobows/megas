emiHidden="false"    UnhideWhenUsed="false" Name="Colorful List Accent 5"/>  <w:LsdException Locked="false" Priority="73" SemiHidden="false"    UnhideWhenUsed="false" Name="Colorful Grid Accent 5"/>  <w:LsdException Locked="false" Priority="60" SemiHidden="false"    UnhideWhenUsed="false" Name="Light Shading Accent 6"/>  <w:LsdException Locked="false" Priority="61" SemiHidden="false"    UnhideWhenUsed="false" Name="Light List Accent 6"/>  <w:LsdException Locked="false" Priority="62" SemiHidden="false"    UnhideWhenUsed="false" Name="Light Grid Accent 6"/>  <w:LsdException Locked="false" Priority="63" SemiHidden="false"    UnhideWhenUsed="false" Name="Medium Shading 1 Accent 6"/>  <w:LsdException Locked="false" Priority="64" SemiHidden="false"    UnhideWhenUsed="false" Name="Medium Shading 2 Accent 6"/>  <w:LsdException Locked="false" Priority="65" SemiHidden="false"    UnhideWhenUsed="false" Name="Medium List 1 Accent 6"/>  <w:LsdException Locked="false" Priority="66" SemiHidden="false"    UnhideWhenUsed="false" Name="Medium List 2 Accent 6"/>  <w:LsdException Locked="false" Priority="67" SemiHidden="false"    UnhideWhenUsed="false" Name="Medium Grid 1 Accent 6"/>  <w:LsdException Locked="false" Priority="68" SemiHidden="false"    UnhideWhenUsed="false" Name="Medium Grid 2 Accent 6"/>  <w:LsdException Locked="false" Priority="69" SemiHidden="false"    UnhideWhenUsed="false" Name="Medium Grid 3 Accent 6"/>  <w:LsdException Locked="false" Priority="70" SemiHidden="false"    UnhideWhenUsed="false" Name="Dark List Accent 6"/>  <w:LsdException Locked="false" Priority="71" SemiHidden="false"    UnhideWhenUsed="false" Name="Colorful Shading Accent 6"/>  <w:LsdException Locked="false" Priority="72" SemiHidden="false"    UnhideWhenUsed="false" Name="Colorful List Accent 6"/>  <w:LsdException Locked="false" Priority="73" SemiHidden="false"    UnhideWhenUsed="false" Name="Colorful Grid Accent 6"/>  <w:LsdException Locked="false" Priority="19" SemiHidden="false"    UnhideWhenUsed="false" QFormat="true" Name="Subtle Emphasis"/>  <w:LsdException Locked="false" Priority="21" SemiHidden="false"    UnhideWhenUsed="false" QFormat="true" Name="Intense Emphasis"/>  <w:LsdException Locked="false" Priority="31" SemiHidden="false"    UnhideWhenUsed="false" QFormat="true" Name="Subtle Reference"/>  <w:LsdException Locked="false" Priority="32" SemiHidden="false"    UnhideWhenUsed="false" QFormat="true" Name="Intense Reference"/>  <w:LsdException Locked="false" Priority="33" SemiHidden="false"    UnhideWhenUsed="false" QFormat="true" Name="Book Title"/>  <w:LsdException Locked="false" Priority="37" Name="Bibliography"/>  <w:LsdException Locked="false" Priority="39" QFormat="true" Name="TOC Heading"/> </w:LatentStyles></xml><![endif]--><!--[if gte mso 10]><style> /* Style Definitions */  table.MsoNormalTable  {mso-style-name:"Tabla normal";  mso-tstyle-rowband-size:0;  mso-tstyle-colband-size:0;  mso-style-noshow:yes;  mso-style-priority:99;  mso-style-qformat:yes;  mso-style-parent:"";  mso-padding-alt:0cm 5.4pt 0cm 5.4pt;  mso-para-margin-top:0cm;  mso-para-margin-right:0cm;  mso-para-margin-bottom:10.0pt;  mso-para-margin-left:0cm;  line-height:115%;  mso-pagination:widow-orphan;  font-size:11.0pt;  font-family:"Calibri","sans-serif";  mso-ascii-font-family:Calibri;  mso-ascii-theme-font:minor-latin;  mso-fareast-font-family:"Times New Roman";  mso-fareast-theme-font:minor-fareast;  mso-hansi-font-family:Calibri;  mso-hansi-theme-font:minor-latin;} </style><![endif]--></div>";s:12:"link_replies";s:190:"http://eduardoposadaescritor.blogspot.com/feeds/1335917996627618988/comments/defaulthttp://eduardoposadaescritor.blogspot.com/2011/08/cronicas-de-cali-el-barrio-chiminangos.html#comment-form";s:9:"link_edit";s:82:"http://www.blogger.com/feeds/8082032128110008795/posts/default/1335917996627618988";s:9:"link_self";s:82:"http://www.blogger.com/feeds/8082032128110008795/posts/default/1335917996627618988";s:4:"link";s:93:"http://eduardoposadaescritor.blogspot.com/2011/08/cronicas-de-cali-el-barrio-chiminangos.html";s:11:"author_name";s:14:"Eduardo Posada";s:10:"author_uri";s:45:"https://plus.google.com/107817794435162273902";s:12:"author_email";s:19:"noreply@blogger.com";}i:11;a:12:{s:2:"id";s:68:"tag:blogger.com,1999:blog-8082032128110008795.post-62888244978074329";s:9:"published";s:29:"2011-05-22T17:12:00.000-07:00";s:7:"updated";s:29:"2013-02-14T20:45:12.644-08:00";s:5:"title";s:69:"Memorias de mi vasectomía de viernes 13 y conversaciones de quirófano";s:12:"atom_content";s:24731:"<div class="MsoNormal" style="text-align: justify;"><span class="Apple-style-span" style="font-family: Calibri, sans-serif;"><span class="Apple-style-span" style="font-size: 15px; line-height: 17px;"></span></span></div><div class="MsoNormal" style="text-align: justify;"><div class="separator" style="clear: both; text-align: center;"><a href="http://2.bp.blogspot.com/-gmU69qoKxaU/UR1_TnrC2ZI/AAAAAAAAAGk/tCJRwHgScOs/s1600/set-para-vasectomia-sin-bisturi.jpg" imageanchor="1" style="margin-left: 1em; margin-right: 1em;"><img border="0" height="240" src="http://2.bp.blogspot.com/-gmU69qoKxaU/UR1_TnrC2ZI/AAAAAAAAAGk/tCJRwHgScOs/s320/set-para-vasectomia-sin-bisturi.jpg" width="320" /></a></div><!--[if gte mso 9]><xml> <w:WordDocument>  <w:View>Normal</w:View>  <w:Zoom>0</w:Zoom>  <w:TrackMoves/>  <w:TrackFormatting/>  <w:HyphenationZone>21</w:HyphenationZone>  <w:PunctuationKerning/>  <w:ValidateAgainstSchemas/>  <w:SaveIfXMLInvalid>false</w:SaveIfXMLInvalid>  <w:IgnoreMixedContent>false</w:IgnoreMixedContent>  <w:AlwaysShowPlaceholderText>false</w:AlwaysShowPlaceholderText>  <w:DoNotPromoteQF/>  <w:LidThemeOther>ES</w:LidThemeOther>  <w:LidThemeAsian>X-NONE</w:LidThemeAsian>  <w:LidThemeComplexScript>X-NONE</w:LidThemeComplexScript>  <w:Compatibility>   <w:BreakWrappedTables/>   <w:SnapToGridInCell/>   <w:WrapTextWithPunct/>   <w:UseAsianBreakRules/>   <w:DontGrowAutofit/>   <w:SplitPgBreakAndParaMark/>   <w:DontVertAlignCellWithSp/>   <w:DontBreakConstrainedForcedTables/>   <w:DontVertAlignInTxbx/>   <w:Word11KerningPairs/>   <w:CachedColBalance/>  </w:Compatibility>  <w:BrowserLevel>MicrosoftInternetExplorer4</w:BrowserLevel>  <m:mathPr>   <m:mathFont m:val="Cambria Math"/>   <m:brkBin m:val="before"/>   <m:brkBinSub m:val="&#45;-"/>   <m:smallFrac m:val="off"/>   <m:dispDef/>   <m:lMargin m:val="0"/>   <m:rMargin m:val="0"/>   <m:defJc m:val="centerGroup"/>   <m:wrapIndent m:val="1440"/>   <m:intLim m:val="subSup"/>   <m:naryLim m:val="undOvr"/>  </m:mathPr></w:WordDocument></xml><![endif]--> <div style="text-align: justify;"><span style="font-family: &quot;Arial&quot;,&quot;sans-serif&quot;;">La mañana del viernes 13 desperté algo ansioso por la vasectomía que me iban a realizar por la tarde y fantaseando con lo divertido que sería ver al urólogo entrar a la sala de operaciones con una máscara de hockey a lo Jason Voorhees en el rostro? aunque si para hacer la broma completa el doctor tuviera un machete en la mano en vez de bisturí creo que yo habría protagonizado un bochornoso incidente corriendo asustado por toda la clínica en bata de cirugía.</span></div><div style="text-align: justify;"><br /></div><div style="text-align: justify;"><span style="font-family: &quot;Arial&quot;,&quot;sans-serif&quot;;">A pesar de ser un procedimiento menor debí llevar un acompañante y mientras el otro tipo que iba a planificar estaba acompañado por su esposa e hijas, yo me aparecí con mi mamá, una venerable ancianita de 73 años. Ella me esperó afuera del baño mientras me colocaba la tradicional bata verde que se amarra por detrás y le deja ver a las enfermeras cosas que no pagaron por ver y luego me acompañó a preguntarle a una de esas enfermeras cómo carajos se ponían las bolsas desechables en los pies pues en el colmo de la&nbsp;inutilidad se nos volvió más difícil encontrarles el derecho que armar un cubo de Rubik con una sola mano.</span></div><div style="text-align: justify;"><br /></div><div style="text-align: justify;"><span style="font-family: &quot;Arial&quot;,&quot;sans-serif&quot;;">Mi operación estaba programada para las 2 p.m. pero revisando la lista de cirugía descubrí con aversión que me precedían una señora a la cuál debían insertarle una malla para sostenerle la vejiga y el tipo que ya había visto antes, quien resultó ser primo de la cuarentona con la vejiga caída. Esa señora tiene (no digo tenía porque infortunadamente sobrevivió a la cirugía) un vocabulario tan soez como el de doña Gloria, la del Metro Cable, por lo que durante los 45 minutos que la hicieron esperar en la sala antes de abrirla tuve&nbsp;que aguantarme la historia criminal de su familia, digna de una novela como La Saga: Negocio de familia.</span></div><div style="text-align: justify;"><br /></div><div style="text-align: justify;"><span style="font-family: &quot;Arial&quot;,&quot;sans-serif&quot;;">Resulta que una de sus hermanas tiene un hijo que a los 16 años cometió un asesinato y tras pagar condena en un centro correccional de Bienestar Familiar se reformó y ahora trabaja reparando y vendiendo motocicletas de segunda. Otra hermana sufrió hace unos meses el asesinato de uno de sus hijos, quien al parecer era sicario y en un ajuste de cuentas (no me resistí a usar ese cliché) le pegaron 9 tiros. El primo, para no ser menos, presumió que en el barrio donde vive hace las fiestas más ruidosas de la ciudad y los policías en vez de llamarle la atención se unen a la rumba para luego irse a patrullar con varios tragos de aguardiente entre pecho y espalda. Cuando comenzaban a desatrasarse de sus proezas sexuales el destino se apiadó de mi y trasladaron a&nbsp; cirugía a esa máquina incontinente de decir improperios. Recuperada la tranquilidad en la sala de espera vi el final del noticiero de RCN, luego Estilo RCN (quería morbosear a Jessica Cediel), y sin mediar palabra me levanté a cambiar el canal para evitar las estúpidas novelas mexicanas de la tarde. Pasando canales encontré Los Cazafantasmas 1, cuando se acabó puse un partido de tenis (me lo dormí un rato, la verdad), luego un canal de videos musicales (¡qué mala es Sale el sol de Shakira!) y mientras planeaba subirle el volumen al máximo al televisor en una canción de Enrique Iglesias como protesta y castigo a las enfermeras ante tanta espera, vinieron por mi compañero de sala, el primo emborracha policías. Veinte minutos después yo estaba bocarriba en una fría mesa de operaciones con una enfermera muy amable lavándome las partes nobles (¿por qué se llaman partes nobles?) con jabón isodine.</span></div><div style="text-align: justify;"><br /></div><div style="text-align: justify;"><span style="font-family: &quot;Arial&quot;,&quot;sans-serif&quot;;">El urólogo se acordaba de mí y le comentó a las enfermeras que yo no quería tener hijos. Una de ellas aseveró que eso era muy respetable (lo que significa en lenguaje diplomático que estaba en desacuerdo con mis acciones) y trató de tranquilizarme pues estaba muy tenso. Antes de la intervención el cirujano te aprieta los testículos y presiona la zona donde va a abrir y cortar. El pinchazo de la anestesia duele poco, pero la impresión de sentir que te están cortando es terriblemente incómoda. Una enfermera de ojos verdes, la instrumentadora, impresionada por que no deseo tener hijos le preguntó al doctor si en caso de volver al pasado se haría la vasectomía evitando tener sus dos niñas. Ante esa pueril pregunta sentí la obligación de contestar por el cirujano afirmando que era absurdo para el doctor imaginarse sin sus dos hijas amadas y ?roto el hielo? estuve durante el resto de mi operación haciendo bromas sobre lo cobardes que somos los hombres y lo afortunados que somos de tener nada más que un par de testículos.</span></div><div style="text-align: justify;"><span style="font-family: &quot;Arial&quot;,&quot;sans-serif&quot;;">&nbsp; </span></div><div style="text-align: justify;"><span style="font-family: &quot;Arial&quot;,&quot;sans-serif&quot;;">En tres meses, tras conocer los resultados de un espermograma, sabré si la vasectomía fue un éxito o deberé ser nuevamente sometido a una intervención quirúrgica para mantener mis espermatozoides alejados del mundo exterior. Es poco probable que mis conductos epididimales hayan creado pequeñas derivaciones que comuniquen sus extremos cortados, pero si mis testículos resultan tener las mismas habilidades regenerativas de los testículos de Wolverine, el personaje de los X-men, deberé ser sometido a otra cirugía ambulatoria, aunque prefiera ver a Piedad Córdoba restituida en el Senado antes que volver al quirófano.</span></div><!--[if gte mso 9]><xml> <w:LatentStyles DefLockedState="false" DefUnhideWhenUsed="true"   DefSemiHidden="true" DefQFormat="false" DefPriority="99"   LatentStyleCount="267">  <w:LsdException Locked="false" Priority="0" SemiHidden="false"    UnhideWhenUsed="false" QFormat="true" Name="Normal"/>  <w:LsdException Locked="false" Priority="9" SemiHidden="false"    UnhideWhenUsed="false" QFormat="true" Name="heading 1"/>  <w:LsdException Locked="false" Priority="9" QFormat="true" Name="heading 2"/>  <w:LsdException Locked="false" Priority="9" QFormat="true" Name="heading 3"/>  <w:LsdException Locked="false" Priority="9" QFormat="true" Name="heading 4"/>  <w:LsdException Locked="false" Priority="9" QFormat="true" Name="heading 5"/>  <w:LsdException Locked="false" Priority="9" QFormat="true" Name="heading 6"/>  <w:LsdException Locked="false" Priority="9" QFormat="true" Name="heading 7"/>  <w:LsdException Locked="false" Priority="9" QFormat="true" Name="heading 8"/>  <w:LsdException Locked="false" Priority="9" QFormat="true" Name="heading 9"/>  <w:LsdException Locked="false" Priority="39" Name="toc 1"/>  <w:LsdException Locked="false" Priority="39" Name="toc 2"/>  <w:LsdException Locked="false" Priority="39" Name="toc 3"/>  <w:LsdException Locked="false" Priority="39" Name="toc 4"/>  <w:LsdException Locked="false" Priority="39" Name="toc 5"/>  <w:LsdException Lock