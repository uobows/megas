n="false"    UnhideWhenUsed="false" Name="Light Grid Accent 5"/>  <w:LsdException Locked="false" Priority="63" SemiHidden="false"    UnhideWhenUsed="false" Name="Medium Shading 1 Accent 5"/>  <w:LsdException Locked="false" Priority="64" SemiHidden="false"    UnhideWhenUsed="false" Name="Medium Shading 2 Accent 5"/>  <w:LsdException Locked="false" Priority="65" SemiHidden="false"    UnhideWhenUsed="false" Name="Medium List 1 Accent 5"/>  <w:LsdException Locked="false" Priority="66" SemiHidden="false"    UnhideWhenUsed="false" Name="Medium List 2 Accent 5"/>  <w:LsdException Locked="false" Priority="67" SemiHidden="false"    UnhideWhenUsed="false" Name="Medium Grid 1 Accent 5"/>  <w:LsdException Locked="false" Priority="68" SemiHidden="false"    UnhideWhenUsed="false" Name="Medium Grid 2 Accent 5"/>  <w:LsdException Locked="false" Priority="69" SemiHidden="false"    UnhideWhenUsed="false" Name="Medium Grid 3 Accent 5"/>  <w:LsdException Locked="false" Priority="70" SemiHidden="false"    UnhideWhenUsed="false" Name="Dark List Accent 5"/>  <w:LsdException Locked="false" Priority="71" SemiHidden="false"    UnhideWhenUsed="false" Name="Colorful Shading Accent 5"/>  <w:LsdException Locked="false" Priority="72" SemiHidden="false"    UnhideWhenUsed="false" Name="Colorful List Accent 5"/>  <w:LsdException Locked="false" Priority="73" SemiHidden="false"    UnhideWhenUsed="false" Name="Colorful Grid Accent 5"/>  <w:LsdException Locked="false" Priority="60" SemiHidden="false"    UnhideWhenUsed="false" Name="Light Shading Accent 6"/>  <w:LsdException Locked="false" Priority="61" SemiHidden="false"    UnhideWhenUsed="false" Name="Light List Accent 6"/>  <w:LsdException Locked="false" Priority="62" SemiHidden="false"    UnhideWhenUsed="false" Name="Light Grid Accent 6"/>  <w:LsdException Locked="false" Priority="63" SemiHidden="false"    UnhideWhenUsed="false" Name="Medium Shading 1 Accent 6"/>  <w:LsdException Locked="false" Priority="64" SemiHidden="false"    UnhideWhenUsed="false" Name="Medium Shading 2 Accent 6"/>  <w:LsdException Locked="false" Priority="65" SemiHidden="false"    UnhideWhenUsed="false" Name="Medium List 1 Accent 6"/>  <w:LsdException Locked="false" Priority="66" SemiHidden="false"    UnhideWhenUsed="false" Name="Medium List 2 Accent 6"/>  <w:LsdException Locked="false" Priority="67" SemiHidden="false"    UnhideWhenUsed="false" Name="Medium Grid 1 Accent 6"/>  <w:LsdException Locked="false" Priority="68" SemiHidden="false"    UnhideWhenUsed="false" Name="Medium Grid 2 Accent 6"/>  <w:LsdException Locked="false" Priority="69" SemiHidden="false"    UnhideWhenUsed="false" Name="Medium Grid 3 Accent 6"/>  <w:LsdException Locked="false" Priority="70" SemiHidden="false"    UnhideWhenUsed="false" Name="Dark List Accent 6"/>  <w:LsdException Locked="false" Priority="71" SemiHidden="false"    UnhideWhenUsed="false" Name="Colorful Shading Accent 6"/>  <w:LsdException Locked="false" Priority="72" SemiHidden="false"    UnhideWhenUsed="false" Name="Colorful List Accent 6"/>  <w:LsdException Locked="false" Priority="73" SemiHidden="false"    UnhideWhenUsed="false" Name="Colorful Grid Accent 6"/>  <w:LsdException Locked="false" Priority="19" SemiHidden="false"    UnhideWhenUsed="false" QFormat="true" Name="Subtle Emphasis"/>  <w:LsdException Locked="false" Priority="21" SemiHidden="false"    UnhideWhenUsed="false" QFormat="true" Name="Intense Emphasis"/>  <w:LsdException Locked="false" Priority="31" SemiHidden="false"    UnhideWhenUsed="false" QFormat="true" Name="Subtle Reference"/>  <w:LsdException Locked="false" Priority="32" SemiHidden="false"    UnhideWhenUsed="false" QFormat="true" Name="Intense Reference"/>  <w:LsdException Locked="false" Priority="33" SemiHidden="false"    UnhideWhenUsed="false" QFormat="true" Name="Book Title"/>  <w:LsdException Locked="false" Priority="37" Name="Bibliography"/>  <w:LsdException Locked="false" Priority="39" QFormat="true" Name="TOC Heading"/> </w:LatentStyles></xml><![endif]--><!--[if gte mso 10]><style> /* Style Definitions */  table.MsoNormalTable  {mso-style-name:"Tabla normal";  mso-tstyle-rowband-size:0;  mso-tstyle-colband-size:0;  mso-style-noshow:yes;  mso-style-priority:99;  mso-style-qformat:yes;  mso-style-parent:"";  mso-padding-alt:0cm 5.4pt 0cm 5.4pt;  mso-para-margin-top:0cm;  mso-para-margin-right:0cm;  mso-para-margin-bottom:10.0pt;  mso-para-margin-left:0cm;  line-height:115%;  mso-pagination:widow-orphan;  font-size:11.0pt;  font-family:"Calibri","sans-serif";  mso-ascii-font-family:Calibri;  mso-ascii-theme-font:minor-latin;  mso-hansi-font-family:Calibri;  mso-hansi-theme-font:minor-latin;  mso-bidi-font-family:"Times New Roman";  mso-bidi-theme-font:minor-bidi;} </style><![endif]--> <br /><div align="center" class="MsoNormal" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: center;"><b style="mso-bidi-font-weight: normal;"><u><span style="font-family: &quot;Times New Roman&quot;,&quot;serif&quot;; font-size: 12.0pt; line-height: 115%;"><br /></span></u></b></div><div class="MsoNormal" style="margin-bottom: 0.0001pt; text-align: center;"><u><span style="font-family: &quot;Times New Roman&quot;,&quot;serif&quot;; font-size: 12.0pt; line-height: 115%;">En referencia a "Las leyes de adoctrinamiento sexual" de Hazteoir.org y ese autocar naranja, expongo mi opinión:</span></u></div><div class="MsoNormal" style="margin-bottom: 0.0001pt; text-align: center;"><u><span style="font-family: &quot;Times New Roman&quot;,&quot;serif&quot;; font-size: 12.0pt; line-height: 115%;"><br /></span></u></div><div class="MsoNormal" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: justify; text-justify: inter-ideograph;"><br /></div><div class="MsoNormal" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: justify; text-justify: inter-ideograph;"><span style="font-family: &quot;Times New Roman&quot;,&quot;serif&quot;; font-size: 12.0pt; line-height: 115%;"></span><span style="font-family: &quot;Times New Roman&quot;,&quot;serif&quot;; font-size: 12.0pt; line-height: 115%;">Adoctrinamiento. Eso es de lo que se trata. La eterna lucha para ver quién es mejor manipulador, ver quién se lleva el pato al agua y hace prevalecer sus ideales, únicos e indiscutibles, por encima de cualquier otra forma de pensamiento o actuación.</span></div><div class="MsoNormal" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: justify; text-justify: inter-ideograph;"><span style="font-family: &quot;Times New Roman&quot;,&quot;serif&quot;; font-size: 12.0pt; line-height: 115%;">Voy a intentar ser todo lo más respetuoso que me permita mi carácter, mis propias ideas, y el hacha que llevo a la espalda. Sé que hay gente que comulga con lo expuesto en el texto que voy a destripar, algunos amigos incluso, pero si algo busco con estas palabras es el debate y no la falta de respeto. Que lo consiga es otra cosa. </span></div><div class="MsoNormal" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: justify; text-justify: inter-ideograph;"><span style="font-family: &quot;Times New Roman&quot;,&quot;serif&quot;; font-size: 12.0pt; line-height: 115%;">Iré escribiendo en esta parrafada tal y como vaya leyendo y reflexionando sobre lo leído en ?¿Sabes lo que quieren enseñarle a su hijo en el colegio??, ese panfleto que, abanderado por un autocar que ofende menos que 120 caracteres en Twitter, según la ley, se lanza a la calle en busca de poner orden en una sociedad que empieza a no ser del gusto de algunos (como si al resto nos gustase tal y como está, vamos).</span></div><div class="MsoNormal" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: justify; text-justify: inter-ideograph;"><span style="font-family: &quot;Times New Roman&quot;,&quot;serif&quot;; font-size: 12.0pt; line-height: 115%;">Empiezo, pues:</span></div><div class="MsoNormal" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: justify; text-justify: inter-ideograph;"><br /></div><div class="MsoNormal" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: justify; text-justify: inter-ideograph;"><span style="font-family: &quot;Times New Roman&quot;,&quot;serif&quot;; font-size: 12.0pt; line-height: 115%;">Joder, ya empezamos mal si quiero mantener un tono serio y de mentalidad abierta. Dice que están proliferando leyes aprobadas en el ámbito de las CCAA. Cojones, eso es democracia, ¿no? Menos mal que ese no es el tema a tratar ahora, pero tela con esta peña. Sigo.</span></div><div class="MsoNormal" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: justify; text-justify: inter-ideograph;"><span style="font-family: &quot;Times New Roman&quot;,&quot;serif&quot;; font-size: 12.0pt; line-height: 115%;">Ja, ja? Ahora empieza el humor. Sí, es cierto que todas las personas tienen (en teoría) los mismo derechos, y el colectivo (no me gusta ese término, pero bueno) LGTBI está dentro de ese ?todas las personas?, pero resulta que cuando alguien es atacado, o su naturaleza se pone en duda (y se ataca o se quiere prohibir, manipular o reprimir), es normal que se haga hincapié en su defensa y reconocimiento. Argumenta este panfleto que legislar así vulnera el derecho fundamental del resto de ciudadanos. No sé qué decirles, pues esas ?leyes? no me perjudican en nada? Es más, sí me joden bastante otras, como las que eximen a la Iglesia de pagar el IBI, por poner un ejemplo.</span></div><div class="MsoNormal" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: justify; text-justify: inter-ideograph;"><span style="font-family: &quot;Times New Roman&quot;,&quot;serif&quot;; font-size: 12.0pt; line-height: 115%;">Argumentan que todo esto es un rechazo al sexo biológico como diferenciador. No sé, pero yo creo que todo tipo de relación sexual es bien biológica? pero vamos, que no voy a ponerme ahora a exponer una tesis sobre el sexo y tal. También infieren en que hay un ataque al binomio natural hombre-mujer. La verdad, nunca me he encontrado a nadie del colectivo LGTBI que me ataque o menosprecie por ser heterosexual. Además, señalo ya que desde mi condición sexual, que no soy defensor de ese binomio. Creo que cada uno es libre de vivir como le dé la gana, siempre y cuando no perjudique a nadie. Esta peña (los del panfleto y el autocar naranja) se toman una libertad, que debe ser por derecho, como un ataque a sus valores. A veces, leyendo cosas así, me da la sensación de que este sector tan conservador no ve que el hecho de ser diferente a ellos no implica ser un enemigo que quiere hacer lo que ellos sí pretenden hacer: Reconvertir.</span></div><div class="MsoNormal" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: justify; text-justify: inter-ideograph;"><span style="font-family: &quot;Times New Roman&quot;,&quot;serif&quot;; font-size: 12.0pt; line-height: 115%;">Hablan de ?Dogma?, cuando estos mismos son los que esgrimen uno contra todo aquello que no va en su línea de pensamiento. </span></div><div class="MsoNormal" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: justify; text-justify: inter-ideograph;"><br /></div><div class="MsoNormal" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: justify; text-justify: inter-ideograph;"><span style="font-family: &quot;Times New Roman&quot;,&quot;serif&quot;; font-size: 12.0pt; line-height: 115%;">La cosa sigue con una enumeración de factores que se ven perjudicados por esta nueva ola de pensamiento ?libertina?. Manda huevos, que diría uno de los suyos.</span></div><div class="MsoNormal" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: justify; text-justify: inter-ideograph;"><span style="font-family: &quot;Times New Roman&quot;,&quot;serif&quot;; font-size: 12.0pt; line-height: 115%;">No voy a copiar los cinco puntos, todo sea que me denuncien por reproducción del contenido protegido por el copyright, pero vamos, es para reír y no echar gota.</span></div><div class="MsoNormal" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: justify; text-justify: inter-ideograph;"><span style="font-family: &quot;Times New Roman&quot;,&quot;serif&quot;; font-size: 12.0pt; line-height: 115%;">Resulta que, para ellos, estamos al límite del precipicio, a punto de acabar con los derechos de los padres (y madres ?permitidme que no haga distinción de sexos, por favor, que esto se puede alargar mucho) a dar una educación adecuada a sus convicciones y creencias. Claro, ese es kit de la cuestión: Manipulación contra <span style="mso-spacerun: yes;">&nbsp;</span>manipulación. Si a sus vástagos les meten ideas en el coco que no sean las suyas, la raza acabará por desaparecer. Hay que perpetuar la especie, por dios (entiéndase el sarcasmo). Esgrimen también que esto desorienta a los niños en su sexualidad, que discrimina a la familia natural, imponiendo a los padres, maestros y centros de enseñanza modelos familiares no adecuados. Y la monda: Que el dinero público se va a adoctrinar sexualmente a sus hijos a través de las organizaciones LGTBI. Es para flipar, ¿no? </span></div><div class="MsoNormal" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: justify; text-justify: inter-ideograph;"><span style="font-family: &quot;Times New Roman&quot;,&quot;serif&quot;; font-size: 12.0pt; line-height: 115%;">Cualquier tipo de enseñanza practicada en España (salvo alguna excepción innovadora y <span style="mso-spacerun: yes;">&nbsp;</span>minoritaria) es tremendamente manipuladora, dogmática y cerrada. El libre pensamiento ofende, el rechazo a los deberes en casa ofende, y el contestar y protestar ofende. Salirse de la regla estipulada es pecado. </span></div><div class="MsoNormal" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: justify; text-justify: inter-ideograph;"><span style="font-family: &quot;Times New Roman&quot;,&quot;serif&quot;; font-size: 12.0pt; line-height: 115%;">Sexualmente hablando, y es lo que parece que más le importa a esta gente, cabría recordarles que cualquier Gay (recójase en esta palabra a todo el colectivo LGTBI, por favor) nació de una madre que se quedó preñada con semen, o sea, de un hombre (aquí no interviene un palomo). Y esa persona nació ya con una orientación sexual definida. Yo recuerdo a compañeros de escuela que desde pequeños ya habían despertado su sexualidad con una clara orientación, y su familia era ?convencional?, de esas que tanto gustan a los conservadores. Esos compañeros míos no tuvieron influencias de ningún tipo (estamos en los años de la EGB), ni en la escuela ni en sus casa, ni en la televisión (donde triunfaban las pelis de Pajares y Esteso). Entonces? ¿Qué pasa? ¿Quién los convirtió en tortilleras y maricones, que se les llamaba antes? No, señores míos, la orientación sexual es algo con lo que se nace. Que luego uno quiera experimentar, pues cojonudo. La libertad está para eso. O como digo yo: Que cada uno se meta en la boca lo que quiera.</span></div><div class="MsoNormal" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: justify; text-justify: inter-ideograph;"><span style="font-family: &quot;Times New Roman&quot;,&quot;serif&quot;; font-size: 12.0pt; line-height: 115%;">Me gustaría recordar, antes de seguir, que en la antigüedad, antes incluso de las tradiciones que se defienden en el panfleto que estoy analizando, el culto al cuerpo muchas veces veneraba la relación entre personas del mismo sexo, dejando el ?tradicional? hombre-mujer para la reproducción. Pero vamos, que es más normal que un cura se folle a un niño que no que dos hombres se peguen un buen festival de cama, porque se aman y quieren disfrutar a tope de sus cuerpos.</span></div><div class="MsoNormal" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: justify; text-justify: inter-ideograph;"><br /></div><div class="MsoNormal" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: justify; text-justify: inter-ideograph;"><span style="font-family: &quot;Times New Roman&quot;,&quot;serif&quot;; font-size: 12.0pt; line-height: 115%;">El siguiente punto del librillo se pregunta en qué nos afecta todo este libertinaje. </span></div><div class="MsoNormal" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: justify; text-justify: inter-ideograph;"><span style="font-family: &quot;Times New Roman&quot;,&quot;serif&quot;; font-size: 12.0pt; line-height: 115%;">Está claro que la pregunta está muy bien formulada, porque, aunque quieren hablar al público en general, hace rato que me ha quedado patente que se hablan a sí mismos. Eso es de agradecer, pues por un momento pensaba que querían adoctrinarme, válgame dios, y eso no está de acuerdo con sus formas y métodos tradicionales.</span></div><div class="MsoNormal" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: justify; text-justify: inter-ideograph;"><span style="font-family: &quot;Times New Roman&quot;,&quot;serif&quot;; font-size: 12.0pt; line-height: 115%;">Dicen que todo el embrollo afecta a nuestros hijos. Mencionan que estas leyes modernas introducen en las aulas una ideología que modifican el proyecto educativo. Yo creo que es mejor eso que no que un cura te introduzca otra cosa por otro sitio, pero vamos, que se repiten más que el ajo (y me hacen repetirme a mí). En 10 páginas he leído lo mismo con diferentes palabras. Creo que van parcos de argumentos sólidos. Sigo, igualmente, vaya que otro redactor tome el relevo y me sorprenda con un giro argumental. Quedan 36 páginas. Creo que pueden reconducir la trama.</span></div><div class="MsoNormal" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: justify; text-justify: inter-ideograph;"><br /></div><div class="MsoNormal" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: justify; text-justify: inter-ideograph;"><span style="font-family: &quot;Times New Roman&quot;,&quot;serif&quot;; font-size: 12.0pt; line-height: 115%;">Pues no. Siguen enumerando leyes y artículos comprendidos en los estatutos de algunas comunidades. Todo, claro, a libre interpretación.</span></div><div class="MsoNormal" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: justify; text-justify: inter-ideograph;"><span style="font-family: &quot;Times New Roman&quot;,&quot;serif&quot;; font-size: 12.0pt; line-height: 115%;">Insisten en el choque de estas ?nuevas enseñanzas? con sus valores. Los mismo que hasta ahora, sin argumentos concluyentes. Esto es más aburrido que matar a una vaca a besos. Empiezo a arrepentirme de no haberles insultado directamente y dedicarme a perder la tarde redactando esto.</span></div><div class="MsoNormal" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: justify; text-justify: inter-ideograph;"><br /></div><div class="MsoNormal" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: justify; text-justify: inter-ideograph;"><span style="font-family: &quot;Times New Roman&quot;,&quot;serif&quot;; font-size: 12.0pt; line-height: 115%;">Ey, hostia, que empieza ?El club de la comedia?:</span></div><div class="MsoNormal" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: justify; text-justify: inter-ideograph;"><span style="font-family: &quot;Times New Roman&quot;,&quot;serif&quot;; font-size: 12.0pt; line-height: 115%;">Dicen que la libertad de conciencia se encuentra hoy amenazada, apoyada esta pandemia por todos los partidos (joder, qué manera de camuflarse entre la foresta, jajaja). ¿En serio? ¿Habla de libertad el carcelero? </span></div><div class="MsoNormal" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: justify; text-justify: inter-ideograph;"><br /></div><div class="MsoNormal" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: justify; text-justify: inter-ideograph;"><span style="font-family: &quot;Times New Roman&quot;,&quot;serif&quot;; font-size: 12.0pt; line-height: 115%;">Ahora llega la hora de mostrar cómo nos quieren cambiar la mente desde el colectivo Gay. Mentan, con ejemplos gráficos y reproducción de textos (citando las fuentes), varias obras que, a mi modo de ver, intentan normalizar los problemas de niños y niñas que no comprenden el rechazo por parte de estos retrógrados, de algunos de sus compañeros de escuela o, sencillamente, por qué se sienten niño o niña si son físicamente lo contrario.</span></div><div class="MsoNormal" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: justify; text-justify: inter-ideograph;"><span style="font-family: &quot;Times New Roman&quot;,&quot;serif&quot;; font-size: 12.0pt; line-height: 115%;">Para mí, estos cuentos, como ?Mi princesito?, de Cheryl Kilodavis, o ?George?, de Alex Gino (los dos primeros que enumeran), son guías prácticas para COMPRENDER, no para adoctrinar. No voy a dar más detalles sobre mi opinión, sencillamente creo que queda claro quién manipula y quién no en base a estos ejemplos.</span></div><div class="MsoNormal" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: justify; text-justify: inter-ideograph;"><span style="font-family: &quot;Times New Roman&quot;,&quot;serif&quot;; font-size: 12.0pt; line-height: 115%;">En este apartado ya hay una reacción más visible al orden que pretenden perpetuar. El sistema familiar sale a la palestra, manteniendo que la base de toda civilización normal es el binomio padre-madre. En mi humilde opinión, mientras un crío tenga el soporte afectivo, emocional, educativo y social, qué más da que en su casa viva con un padre y una madre, dos padres, dos madres, una madre sola o un padre solo. Qué más da si sus progenitores son heterosexuales, homosexuales, lesbianas, etc. Cago en la puta, lo principal son ellos, los pequeños, que necesitan amor. Tanto da quién se lo dé. Imagino que para esta panda de australopitecos es mejor que los niños se pudran en un orfanato (por ejemplo) que no que vivan y sean queridos en un hogar ?inusual?. Ya se pueden ir a la mierda.</span></div><div class="MsoNormal" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: justify; text-justify: inter-ideograph;"><span style="font-family: &quot;Times New Roman&quot;,&quot;serif&quot;; font-size: 12.0pt; line-height: 115%;">Siguiendo con esta basura de panfleto, llegamos a la página 20, donde la toman con la AMPA, dándoles caña por dejarse convencer de que la tradición es mala. Luego van a por el profesorado. Como veis, están dentro de un bucle argumental.</span></div><div class="MsoNormal" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: justify; text-justify: inter-ideograph;"><span style="font-family: &quot;Times New Roman&quot;,&quot;serif&quot;; font-size: 12.0pt; line-height: 115%;">Comentan que los colegios están obligados a tomar esas medidas educativas impuestas, y que eso modifica las actitudes en los espacios comunes, como vestuarios y tal. Hablan incluso de leyes radicales, que hacen que la administración pueda pasar por encima de de la voluntad familiar en casos de ?trastornos?. En serio, es para flipar. No sé qué mierda se mete esta peña en vena, pero que empiecen dejarlo, que eso seguro que acaba pudriéndoles los dientes.</span></div><div class="MsoNormal" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: justify; text-justify: inter-ideograph;"><span style="font-family: &quot;Times New Roman&quot;,&quot;serif&quot;; font-size: 12.0pt; line-height: 115%;">Sigue la cosa con una serie de preguntas sin respuesta. Esto sí me voy a permitir el lujo de reproducirlo, más que nada en un afán de darles un poco de mi modesta luz.</span></div><div class="MsoNormal" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: justify; text-justify: inter-ideograph;"><span style="font-family: &quot;Times New Roman&quot;,&quot;serif&quot;; font-size: 12.0pt; line-height: 115%;">Ante todo, dejo claro que no tengo hijos, pero creo que sí dos dedos de frente. Comencemos:</span></div><div class="MsoNormal" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: justify; text-justify: inter-ideograph;"><br /></div><div class="MsoNormal" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: justify; text-justify: inter-ideograph;"><b style="mso-bidi-font-weight: normal;"><span style="font-family: &quot;Times New Roman&quot;,&quot;serif&quot;; font-size: 12.0pt; line-height: 115%;">Pregunta:</span></b><span style="font-family: &quot;Times New Roman&quot;,&quot;serif&quot;; font-size: 12.0pt; line-height: 115%;">¿Qué son situaciones de sufrimiento e indefensión?</span></div><div class="MsoNormal" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: justify; text-justify: inter-ideograph;"><b style="mso-bidi-font-weight: normal;"><span style="font-family: &quot;Times New Roman&quot;,&quot;serif&quot;; font-size: 12.0pt; line-height: 115%;">Respuesta:</span></b><span style="font-family: &quot;Times New Roman&quot;,&quot;serif&quot;; font-size: 12.0pt; line-height: 115%;">Son aquellas en las que el niño lo pasa mal, ya sea por incomprensión, abandono, humillación, desprecio, marginación, etc. Ejemplo: Si un muchacho se siente chica, sufre simplemente por el hecho de no poder expresarlo por miedo a la reprimenda familiar, social o por lo anteriormente enumerado. Los hay que tienen el valor para dar la cara y ?salir del armario?, y luego sufren las consecuencias. Otros no tienen ese ánimo, y permanecen encerrados en sí mismos? o representando el papel del hijo ?correcto?. Todo eso es sufrimiento e indefensión.</span></div><div class="MsoNormal" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: justify; text-justify: inter-ideograph;"><br /></div><div class="MsoNormal" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: justify; text-justify: inter-ideograph;"><b style="mso-bidi-font-weight: normal;"><span style="font-family: &quot;Times New Roman&quot;,&quot;serif&quot;; font-size: 12.0pt; line-height: 115%;">P:</span></b><span style="font-family: &quot;Times New Roman&quot;,&quot;serif&quot;; font-size: 12.0pt; line-height: 115%;">¿Qué entienden estas leyes por un grave perjuicio al menor?</span></div><div class="MsoNormal" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: justify; text-justify: inter-ideograph;"><b style="mso-bidi-font-weight: normal;"><span style="font-family: &quot;Times New Roman&quot;,&quot;serif&quot;; font-size: 12.0pt; line-height: 115%;">R:</span></b><span style="font-family: &quot;Times New Roman&quot;,&quot;serif&quot;; font-size: 12.0pt; line-height: 115%;">Entiende que ese miedo y sufrimiento no es sano. Ni para menores ni mayores, eso hay que dejarlo claro. Si un mayor puede verse perjudicado, hasta el punto de perder un trabajo o ser apaleado en plena calle, imaginad un chavalín. Hay preguntas que ofenden.</span></div><div class="MsoNormal" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: justify; text-justify: inter-ideograph;"><br /></div><div class="MsoNormal" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: justify; text-justify: inter-ideograph;"><b style="mso-bidi-font-weight: normal;"><span style="font-family: &quot;Times New Roman&quot;,&quot;serif&quot;; font-size: 12.0pt; line-height: 115%;">P:</span></b><span style="font-family: &quot;Times New Roman&quot;,&quot;serif&quot;; font-size: 12.0pt; line-height: 115%;">¿Y si lo realmente dañino y perjudicial para el menor es abandonarlo a su libre voluntad, sin evaluación especializada ni orientación médica?</span></div><div class="MsoNormal" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: justify; text-justify: inter-ideograph;"><b style="mso-bidi-font-weight: normal;"><span style="font-family: &quot;Times New Roman&quot;,&quot;serif&quot;; font-size: 12.0pt; line-height: 115%;">R:</span></b><span style="font-family: &quot;Times New Roman&quot;,&quot;serif&quot;; font-size: 12.0pt; line-height: 115%;">Vamos a ver: ¿Orientación médica? ¿Lo quieren tratar de enfermedad? ¿Evaluación especializada? ¿Algún especialista en la sala, por favor? ¿Libre voluntad? ¡¡¡Por las barbas de Odín!!! ¡¡¡¿Qué locura es esa?!!! En serio, ¿Se creen que un niño es un muñeco al que manipular como quieran? Bueno, esta vez soy yo el ingenuo. Leyendo lo que estoy leyendo sí, está claro que lo creen. Un niño es un producto, no una persona con pensamiento, sentimientos y anhelos, capaz de disfrutar y sufrir, de ser autónomo y saber lo que quiere y lo que no, lo que le gusta y lo que detesta? de saber qué y quién es, y de qué y quién no.</span></div><div class="MsoNormal" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: justify; text-justify: inter-ideograph;"><span style="font-family: &quot;Times New Roman&quot;,&quot;serif&quot;; font-size: 12.0pt; line-height: 115%;">Contestando a la pregunta: No, eso que enumeran no es dañino ni perjudicial.</span></div><div class="MsoNormal" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: justify; text-justify: inter-ideograph;"><br /></div><div class="MsoNormal" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: justify; text-justify: inter-ideograph;"><b style="mso-bidi-font-weight: normal;"><span style="font-family: &quot;Times New Roman&quot;,&quot;serif&quot;; font-size: 12.0pt; line-height: 115%;">P:</span></b><span style="font-family: &quot;Times New Roman&quot;,&quot;serif&quot;; font-size: 12.0pt; line-height: 115%;">¿Quién puede asegurar que estas medidas de hoy no serán la causa de un mayor sufrimiento futuro?</span></div><div class="MsoNormal" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: justify; text-justify: inter-ideograph;"><b style="mso-bidi-font-weight: normal;"><span style="font-family: &quot;Times New Roman&quot;,&quot;serif&quot;; font-size: 12.0pt; line-height: 115%;">R:</span></b><span style="font-family: &quot;Times New Roman&quot;,&quot;serif&quot;; font-size: 12.0pt; line-height: 115%;">Nadie puede asegurarlo mientras haya gente que les ataque con, por ejemplo, panfletos como el que estoy analizando. Nadie.</span></div><div class="MsoNormal" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: justify; text-justify: inter-ideograph;"><br /></div><div class="MsoNormal" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: justify; text-justify: inter-ideograph;"><span style="font-family: &quot;Times New Roman&quot;,&quot;serif&quot;; font-size: 12.0pt; line-height: 115%;">Lo que sigue a este cuestionario es buenísimo. Muestran el dibujo que ha hecho una niña, donde se ve a Kiko y su novio. Para ellos, esta cría ya ha sido manipulada en la nueva ideología. Brutal, ¿verdad? Es que ya no sé ni qué poner. Flipo. ¿Realmente son tan retrógrados como para ver el mal en un inocente dibujo? Esto no es más que una escena que la muchacha pueda haber visto en la calle. Tal cual. Quizá prefieran que la cría dibuje a políticos corruptos riéndose del pueblo, o a banqueros reclamando pisos hipotecados, dejando a familias (también tradicionales, sí) en la puta calle. En fin?</span></div><div class="MsoNormal" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: justify; text-justify: inter-ideograph;"><br /></div><div class="MsoNormal" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: justify; text-justify: inter-ideograph;"><span style="font-family: &quot;Times New Roman&quot;,&quot;serif&quot;; font-size: 12.0pt; line-height: 115%;">La cosa empieza a enfilar hacia su final señalando que todo esto son verdades judiciales y no científicas. Me meo de risa. Hablan de ciencia cuando, seguramente, ellos son los primeros en negar la evolución darwiniana, la inexistencia del Edén tal y como lo describe la Biblia, o la existencia de una conciencia universal, basada en el Amor incondicional. Me vais a tener que perdonar, pero a estas alturas del texto ya tengo muy clara la postura social y religiosa de los artífices de tamaña iniciativa educativa.</span></div><div class="MsoNormal" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: justify; text-justify: inter-ideograph;"><span style="font-family: &quot;Times New Roman&quot;,&quot;serif&quot;; font-size: 12.0pt; line-height: 115%;">Arremeten contra los jueces, sin más argumentos que el bucle en el que ya comentaba que están inmersos.</span></div><div class="MsoNormal" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: justify; text-justify: inter-ideograph;"><span style="font-family: &quot;Times New Roman&quot;,&quot;serif&quot;; font-size: 12.0pt; line-height: 115%;">Me está siendo tremendamente difícil exponer mi punto de vista sin repetirme, pues es que no dan pie a más argumentos que los que ya llevo redactados. Uf, qué desafío.</span></div><div class="MsoNormal" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: justify; text-justify: inter-ideograph;"><br /></div><div class="MsoNormal" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: justify; text-justify: inter-ideograph;"><span style="font-family: &quot;Times New Roman&quot;,&quot;serif&quot;; font-size: 12.0pt; line-height: 115%;">En el siguiente párrafo sacan a relucir su vertiente política, manifestando que las cosas acabarán siendo a gusto del gobernador de turno. ¿Es que no lo son ya, con todos los asuntos, no sólo con este? ¿Habéis descubierto ahora la piedra filosofal? Vamos, no me vengáis de ingenuos, por Tor.</span></div><div class="MsoNormal" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: justify; text-justify: inter-ideograph;"><br /></div><div class="MsoNormal" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: justify; text-justify: inter-ideograph;"><span style="font-family: &quot;Times New Roman&quot;,&quot;serif&quot;; font-size: 12.0pt; line-height: 115%;">¡Hostias, si pillan cacho hasta lo médicos!</span></div><div class="MsoNormal" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: justify; text-justify: inter-ideograph;"><span style="font-family: &quot;Times New Roman&quot;,&quot;serif&quot;; font-size: 12.0pt; line-height: 115%;">La medicina al servicio de la ideología, titulan un capítulo.</span></div><div class="MsoNormal" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: justify; text-justify: inter-ideograph;"><span style="font-family: &quot;Times New Roman&quot;,&quot;serif&quot;; font-size: 12.0pt; line-height: 115%;">O sea, que lo que ellos entienden por enfermedad es brutalmente masacrado y negado por una conspiración médica al servicio de un colectivo con gran poder mediático. Les ha faltado decir que nos quieren a todos enfermos (que para mí que eso es lo que buscan las farmacéuticas, no sé yo? mmm). Luego dan cifras sobre multas a quienes realicen terapias de conversión y bla, bla, bla.</span></div><div class="MsoNormal" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: justify; text-justify: inter-ideograph;"><br /></div><div class="MsoNormal" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: justify; text-justify: inter-ideograph;"><span style="font-family: &quot;Times New Roman&quot;,&quot;serif&quot;; font-size: 12.0pt; line-height: 115%;">Ya me he cansado de leer estupideces.</span></div><div class="MsoNormal" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: justify; text-justify: inter-ideograph;"><br /></div><div class="MsoNormal" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: justify; text-justify: inter-ideograph;"><span style="font-family: &quot;Times New Roman&quot;,&quot;serif&quot;; font-size: 12.0pt; line-height: 115%;">Bueno, espera, hay un capítulo que seguro que me da juego: ?Una ley que convierte en sospechosa la heterosexualidad?.</span></div><div class="MsoNormal" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: justify; text-justify: inter-ideograph;"><span style="font-family: &quot;Times New Roman&quot;,&quot;serif&quot;; font-size: 12.0pt; line-height: 115%;">Mira, aquí sí voy a estar de acuerdo con ellos. Es más, tengo la solución para que a los heterosexuales no se nos vea como bichos raros, retrógrados, homófobos, anticuados, apolillados y trogloditas: Acabar con sus argumentos de mierda y mandaros de una patada en los cojones al siglo XV.</span></div><div class="MsoNormal" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: justify; text-justify: inter-ideograph;"><br /></div><div class="MsoNormal" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: justify; text-justify: inter-ideograph;"><span style="font-family: &quot;Times New Roman&quot;,&quot;serif&quot;; font-size: 12.0pt; line-height: 115%;">¡Hostias! ¡Que hablan de memoria histórica! Esto sí que no me lo pierdo.</span></div><div class="MsoNormal" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: justify; text-justify: inter-ideograph;"><span style="font-family: &quot;Times New Roman&quot;,&quot;serif&quot;; font-size: 12.0pt; line-height: 115%;">Se sorprenden que se hable del franquismo como un enemigo de los homosexuales, y que se diga que la gente no se expresa y defiende sus principios sobre este tema por ser tachados de franquistas. La madre que los parió. Ja, ja, ja. Qué cinismo más cultivado. Qué hipocresía más bien enmascarada por un sentimiento victimista y reprimido. Qué hijoputez más descarada.</span></div><div class="MsoNormal" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: justify; text-justify: inter-ideograph;"><br /></div><div class="MsoNormal" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: justify; text-justify: inter-ideograph;"><span style="font-family: &quot;Times New Roman&quot;,&quot;serif&quot;; font-size: 12.0pt; line-height: 115%;">En fin, que me quedan varias páginas por leer, pero viendo el título de los capítulos, insisto nuevamente, no es más que un bucle del que no van a salir. Me da que es como discutir con una pared. Así pues, a modo de finiquito, voy a dejar mi reflexión final, siempre a título personal, preparado para debatir con cualquiera que tenga algo constructivo que aportar al tema de marras:</span></div><div class="MsoNormal" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: justify; text-justify: inter-ideograph;"><br /></div><div class="MsoNormal" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: justify; text-justify: inter-ideograph;"><span style="font-family: &quot;Times New Roman&quot;,&quot;serif&quot;; font-size: 12.0pt; line-height: 115%;">Peña, que las cosas cambian, que el mundo y la sociedad evoluciona, que nada es para siempre (por suerte en la mayoría de casos), y que algo que lleva demasiado tiempo bajo el yugo inquisitorial de una sociedad MANIPULADA, retrógrada y con un lastre enorme llamado Iglesia, ha terminado por salir a flote. No es un problema que a los niños se les abran las puertas del conocimiento, la comprensión, la amistad y el amor. Esa es la solución. Sí. Lo mismo que los autores y adoradores de este panfleto infumable tachan de caos, no es más que una restauración, una vuelta de tuerca que restablece un orden. Somos individuos, únicos, y en eso no debe inmiscuirse nadie. Cada uno es lo que es, siente lo que siente, y lo vive y disfruta como quiere (o así debería ser).</span></div><div class="MsoNormal" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: justify; text-justify: inter-ideograph;"><span style="font-family: &quot;Times New Roman&quot;,&quot;serif&quot;; font-size: 12.0pt; line-height: 115%;">Desde aquí, en palabras de un heterosexual al que le importa una mierda las preferencias sexuales o afectivas de quienes le rodean, mi más sincero apoyo a todos aquellos a los que estas campañas del pleistoceno pretenden llevar de nuevo a la hoguera. </span></div><div class="MsoNormal" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: justify; text-justify: inter-ideograph;"><br /></div><div class="MsoNormal" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: justify; text-justify: inter-ideograph;"><br /></div><div align="right" class="MsoNormal" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: right;"><span style="font-family: &quot;Times New Roman&quot;,&quot;serif&quot;; font-size: 12.0pt; line-height: 115%;">David Arrabal Carrión</span></div></div>";s:12:"link_replies";s:200:"http://elfinaldetodoslosinviernos.blogspot.com/feeds/4839245466452859335/comments/defaulthttp://elfinaldetodoslosinviernos.blogspot.com/2017/02/se-lo-que-quieren-ensenarles-los-ninos.html#comment-form";s:9:"link_edit";s:82:"http://www.blogger.com/feeds/6027972662857348333/posts/default/4839245466452859335";s:9:"link_self";s:82:"http://www.blogger.com/feeds/6027972662857348333/posts/default/4839245466452859335";s:4:"link";s:98:"http://elfinaldetodoslosinviernos.blogspot.com/2017/02/se-lo-que-quieren-ensenarles-los-ninos.html";s:11:"author_name";s:21:"David Arrabal Carrión";s:10:"author_uri";s:45:"https://plus.google.com/106237369997906967710";s:12:"author_email";s:19:"noreply@blogger.com";}i:15;a:12:{s:2:"id";s:70:"tag:blogger.com,1999:blog-6027972662857348333.post-6950673931184810907";s:9:"published";s:29:"2017-02-21T09:13:00.000-08:00";s:7:"updated";s:29:"2017-02-21T09:13:23.200-08:00";s:5:"title";s:64:"Más presentaciones de Sitra Ahra (Sant Boi de Llbregat y Encamp)";s:12:"atom_content";s:1767:"<div dir="ltr" style="text-align: left;" trbidi="on"><div style="text-align: justify;">Siguiendo la gira de presentaciones de <a href="http://clansmaninvictus.wixsite.com/davidarrabalcarrion/sitra-ahra" target="_blank">Sitra Ahra</a>, las siguientes paradas fueron <a href="http://clansmaninvictus.wixsite.com/davidarrabalcarrion/sitra-ahra-en-les-hores-sant-boi" target="_blank">Sant Boi de Llobregat, en la Llibreria Les Hores</a>, y <a href="http://www.comuencamp.ad/noticies/david-arrabal-sorpren-al-public-amb-la-seva-darrera-obra" target="_blank">Encamp, en la Biblioteca Comunal</a>. </div><div style="text-align: justify;"><br /></div><br /><div class="separator" style="clear: both; text-align: center;"><a href="https://3.bp.blogspot.com/-OJZCMC_Xu8Q/WKx0RtTxpzI/AAAAAAAACV8/jtQFq2ou-a09XKurxx6MLUy2pSuJ7mc_gCLcB/s1600/Presentaci%25C3%25B3%2BLes%2BHores.jpg" imageanchor="1" style="margin-left: 1em; margin-right: 1em;"><img alt="" border="0" height="320" src="https://3.bp.blogspot.com/-OJZCMC_Xu8Q/WKx0RtTxpzI/AAAAAAAACV8/jtQFq2ou-a09XKurxx6MLUy2pSuJ7mc_gCLcB/s320/Presentaci%25C3%25B3%2BLes%2BHores.jpg" title="" width="226" /></a><a href="https://1.bp.blogspot.com/-6wmWlIwcPAU/WKx0Vy1nSPI/AAAAAAAACWA/q5ZFbwVELK0U3KUthpBxBKF2ICsuaCSGQCLcB/s1600/Presentaci%25C3%25B3%2BEncamp.jpg" imageanchor="1" style="margin-left: 1em; margin-right: 1em;"><img border="0" height="320" src="https://1.bp.blogspot.com/-6wmWlIwcPAU/WKx0Vy1nSPI/AAAAAAAACWA/q5ZFbwVELK0U3KUthpBxBKF2ICsuaCSGQCLcB/s320/Presentaci%25C3%25B3%2BEncamp.jpg" width="226" /></a></div><div style="text-align: center;"><br /></div><div style="text-align: center;"><a href="http://clansmaninvictus.wixsite.com/davidarrabalcarrion" target="_blank">Para más información, visita mi web.</a></div></div>";s:12:"link_replies";s:199:"http://elfinaldetodoslosinviernos.blogspot.com/feeds/6950673931184810907/comments/defaulthttp://elfinaldetodoslosinviernos.blogspot.com/2017/02/mas-presentaciones-de-sitra-ahra-sant.html#comment-form";s:9:"link_edit";s:82:"http://www.blogger.com/feeds/6027972662857348333/posts/default/6950673931184810907";s:9:"link_self";s:82:"http://www.blogger.com/feeds/6027972662857348333/posts/default/6950673931184810907";s:4:"link";s:97:"http://elfinaldetodoslosinviernos.blogspot.com/2017/02/mas-presentaciones-de-sitra-ahra-sant.html";s:11:"author_name";s:21:"David Arrabal Carrión";s:10:"author_uri";s:45:"https://plus.google.com/106237369997906967710";s:12:"author_email";s:19:"noreply@blogger.com";}i:16;a:12:{s:2:"id";s:70:"tag:blogger.com,1999:blog-6027972662857348333.post-6912629188408827727";s:9:"published";s:29:"2017-01-21T13:20:00.004-08:00";s:7:"updated";s:29:"2017-01-21T13:20:59.627-08:00";s:5:"title";s:19:"Reseña "Sitra Ahra"";s:12:"atom_content";s:726:"<div dir="ltr" style="text-align: left;" trbidi="on"><div style="text-align: justify;">Desde el blog Cinefagia 80, nos llega la primera reseña de "Sitra Ahra", que sale muy bien parada.</div><div style="text-align: justify;">Gracias a Santiago por sus palabras y el espacio dedicado en su blog (el cual os aconsejo seguir).</div><div style="text-align: justify;"><br /></div><div style="text-align: center;">Lee la reseña pinchando<a href="http://cinefagia80.blogspot.com.es/2017/01/libro-sitra-ahra.html" target="_blank"> <span style="font-size: large;"><b>AQUÍ</b></span></a><span style="font-size: large;"><b>.</b></span></div><div style="text-align: center;"><br /></div><div style="text-align: center;">&nbsp;</div></div>";s:12:"link_replies";s:179:"http://elfinaldetodoslosinviernos.blogspot.com/feeds/6912629188408827727/comments/defaulthttp://elfinaldetodoslosinviernos.blogspot.com/2017/01/resena-sitra-ahra.html#comment-form";s:9:"link_edit";s:82:"http://www.blogger.com/feeds/6027972662857348333/posts/default/6912629188408827727";s:9:"link_self";s:82:"http://www.blogger.com/feeds/6027972662857348333/posts/default/6912629188408827727";s:4:"link";s:77:"http://elfinaldetodoslosinviernos.blogspot.com/2017/01/resena-sitra-ahra.html";s:11:"author_name";s:21:"David Arrabal Carrión";s:10:"author_uri";s:45:"https://plus.google.com/106237369997906967710";s:12:"author_email";s:19:"noreply@blogger.com";}i:17;a:12:{s:2:"id";s:70:"tag:blogger.com,1999:blog-6027972662857348333.post-7069411355060053531";s:9:"published";s:29:"2017-01-12T10:45:00.000-08:00";s:7:"updated";s:29:"2017-01-12T10:45:39.405-08:00";s:5:"title";s:62:"Empieza la gira de Sitra Ahra: Barcelona, Festival Rock N Read";s:12:"atom_content";s:1697:"<div dir="ltr" style="text-align: left;" trbidi="on"><div style="text-align: justify;">Aquí tenemos ya el primer evento de la gira de presentaciones de "Sitra Ahra".</div><div style="text-align: justify;">Para empezar, me desplazo con todo el montaje al <a href="https://www.facebook.com/rockandreadfestival/?fref=ts" target="_blank">Festival Rock N Read</a> de Barcelona, que celebra su segunda edición.</div><div style="text-align: justify;">Tendrá lugar los días 28 y 29 de este mes, y me podréis encontrar el sábado 28 a las 12.00h, presentando mi obra. El resto de la jornada estaré por allí, atendiendo la mesa de ventas y lo que haga falta, acompañado por el resto de participantes, escritores y músicos que harán del festival algo muy interesante.</div><div style="text-align: justify;">La entrada para un día cuesta 5,00?, y para los dos, 8,00?, y tendrá lugar en el espacio <a href="https://entradium.com/entradas/festival-rock-read-ii" target="_blank">The Collective</a> (Carrer d'En Roig, 11, Barcelona - En El Raval, vamos).</div><div style="text-align: justify;">Yo de vosotros, amantes de las letras y la música, no me lo perdería.</div><div style="text-align: justify;"><br /></div><div class="separator" style="clear: both; text-align: center;"><a href="https://2.bp.blogspot.com/-Ugnyk3j3ji4/WHfOXsNz_MI/AAAAAAAACVQ/5WEv24bnvSA_0sy3uUezeZue-cPyVgFCACLcB/s1600/Rock%2Bn%2BRead.jpg" imageanchor="1" style="margin-left: 1em; margin-right: 1em;"><img border="0" height="400" src="https://2.bp.blogspot.com/-Ugnyk3j3ji4/WHfOXsNz_MI/AAAAAAAACVQ/5WEv24bnvSA_0sy3uUezeZue-cPyVgFCACLcB/s400/Rock%2Bn%2BRead.jpg" width="282" /></a></div><div style="text-align: justify;"><br /></div></div>";s:12:"link_replies";s:201:"http://elfinaldetodoslosinviernos.blogspot.com/feeds/7069411355060053531/comments/defaulthttp://elfinaldetodoslosinviernos.blogspot.com/2017/01/empieza-la-gira-de-sitra-ahra-barcelona.html#comment-form";s:9:"link_edit";s:82:"http://www.blogger.com/feeds/6027972662857348333/posts/default/7069411355060053531";s:9:"link_self";s:82:"http://www.blogger.com/feeds/6027972662857348333/posts/default/7069411355060053531";s:4:"link";s:99:"http://elfinaldetodoslosinviernos.blogspot.com/2017/01/empieza-la-gira-de-sitra-ahra-barcelona.html";s:11:"author_name";s:21:"David Arrabal Carrión";s:10:"author_uri";s:45:"https://plus.google.com/106237369997906967710";s:12:"author_email";s:19:"noreply@blogger.com";}i:18;a:12:{s:2:"id";s:69:"tag:blogger.com,1999:blog-6027972662857348333.post-102256582856297379";s:9:"published";s:29:"2016-12-30T16:39:00.000-08:00";s:7:"updated";s:29:"2016-12-30T16:39:55.748-08:00";s:5:"title";s:13:"Lecturas 2016";s:12:"atom_content";s:25179:"<div dir="ltr" style="text-align: left;" trbidi="on"><div style="text-align: justify;">Tradicional es ya dejar por aquí el listado de cositas que he leído durante el año que termina. Así pues, sin más dilación, aquí están mis lecturas de 2016. Han sido pocas, comparadas con otros años, pero las mil y una revisiones a mis obras, y a alguna de amigos que aún no se han publicado, han provocado esa merma. Aún y así, he disfrutado, como siempre.</div><div style="text-align: justify;">Variedad ha habido, y mucha. También he leído a gente como Borges, que no había tenido el placer aún, y he vuelto a revisar al gran William Blake (dioses, cómo adoro su estilazo).</div><div style="text-align: justify;">Han caído clásicos, y obras de amigos. Nunca digo cuál ha sido mi libro o relato preferido, y seguiré con esa costumbre. </div><div style="text-align: justify;">Yo recomiendo todas y cada una de estas obras. Luego juzgad vosotros.</div><div style="text-align: justify;"><br /></div><div style="text-align: justify;"><!--[if gte mso 9]><xml> <w:WordDocument>  <w:View>Normal</w:View>  <w:Zoom>0</w:Zoom>  <w:TrackMoves/>  <w:TrackFormatting/>  <w:HyphenationZone>21</w:HyphenationZone>  <w:PunctuationKerning/>  <w:ValidateAgainstSchemas/>  <w:SaveIfXMLInvalid>false</w:SaveIfXMLInvalid>  <w:IgnoreMixedContent>false</w:IgnoreMixedContent>  <w:AlwaysShowPlaceholderText>false</w:AlwaysShowPlaceholderText>  <w:DoNotPromoteQF/>  <w:LidThemeOther>ES</w:LidThemeOther>  <w:LidThemeAsian>X-NONE</w:LidThemeAsian>  <w:Li