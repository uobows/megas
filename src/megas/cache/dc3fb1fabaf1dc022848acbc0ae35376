ame="Medium Shading 1 Accent 5"/>  <w:LsdException Locked="false" Priority="64" SemiHidden="false"    UnhideWhenUsed="false" Name="Medium Shading 2 Accent 5"/>  <w:LsdException Locked="false" Priority="65" SemiHidden="false"    UnhideWhenUsed="false" Name="Medium List 1 Accent 5"/>  <w:LsdException Locked="false" Priority="66" SemiHidden="false"    UnhideWhenUsed="false" Name="Medium List 2 Accent 5"/>  <w:LsdException Locked="false" Priority="67" SemiHidden="false"    UnhideWhenUsed="false" Name="Medium Grid 1 Accent 5"/>  <w:LsdException Locked="false" Priority="68" SemiHidden="false"    UnhideWhenUsed="false" Name="Medium Grid 2 Accent 5"/>  <w:LsdException Locked="false" Priority="69" SemiHidden="false"    UnhideWhenUsed="false" Name="Medium Grid 3 Accent 5"/>  <w:LsdException Locked="false" Priority="70" SemiHidden="false"    UnhideWhenUsed="false" Name="Dark List Accent 5"/>  <w:LsdException Locked="false" Priority="71" SemiHidden="false"    UnhideWhenUsed="false" Name="Colorful Shading Accent 5"/>  <w:LsdException Locked="false" Priority="72" SemiHidden="false"    UnhideWhenUsed="false" Name="Colorful List Accent 5"/>  <w:LsdException Locked="false" Priority="73" SemiHidden="false"    UnhideWhenUsed="false" Name="Colorful Grid Accent 5"/>  <w:LsdException Locked="false" Priority="60" SemiHidden="false"    UnhideWhenUsed="false" Name="Light Shading Accent 6"/>  <w:LsdException Locked="false" Priority="61" SemiHidden="false"    UnhideWhenUsed="false" Name="Light List Accent 6"/>  <w:LsdException Locked="false" Priority="62" SemiHidden="false"    UnhideWhenUsed="false" Name="Light Grid Accent 6"/>  <w:LsdException Locked="false" Priority="63" SemiHidden="false"    UnhideWhenUsed="false" Name="Medium Shading 1 Accent 6"/>  <w:LsdException Locked="false" Priority="64" SemiHidden="false"    UnhideWhenUsed="false" Name="Medium Shading 2 Accent 6"/>  <w:LsdException Locked="false" Priority="65" SemiHidden="false"    UnhideWhenUsed="false" Name="Medium List 1 Accent 6"/>  <w:LsdException Locked="false" Priority="66" SemiHidden="false"    UnhideWhenUsed="false" Name="Medium List 2 Accent 6"/>  <w:LsdException Locked="false" Priority="67" SemiHidden="false"    UnhideWhenUsed="false" Name="Medium Grid 1 Accent 6"/>  <w:LsdException Locked="false" Priority="68" SemiHidden="false"    UnhideWhenUsed="false" Name="Medium Grid 2 Accent 6"/>  <w:LsdException Locked="false" Priority="69" SemiHidden="false"    UnhideWhenUsed="false" Name="Medium Grid 3 Accent 6"/>  <w:LsdException Locked="false" Priority="70" SemiHidden="false"    UnhideWhenUsed="false" Name="Dark List Accent 6"/>  <w:LsdException Locked="false" Priority="71" SemiHidden="false"    UnhideWhenUsed="false" Name="Colorful Shading Accent 6"/>  <w:LsdException Locked="false" Priority="72" SemiHidden="false"    UnhideWhenUsed="false" Name="Colorful List Accent 6"/>  <w:LsdException Locked="false" Priority="73" SemiHidden="false"    UnhideWhenUsed="false" Name="Colorful Grid Accent 6"/>  <w:LsdException Locked="false" Priority="19" SemiHidden="false"    UnhideWhenUsed="false" QFormat="true" Name="Subtle Emphasis"/>  <w:LsdException Locked="false" Priority="21" SemiHidden="false"    UnhideWhenUsed="false" QFormat="true" Name="Intense Emphasis"/>  <w:LsdException Locked="false" Priority="31" SemiHidden="false"    UnhideWhenUsed="false" QFormat="true" Name="Subtle Reference"/>  <w:LsdException Locked="false" Priority="32" SemiHidden="false"    UnhideWhenUsed="false" QFormat="true" Name="Intense Reference"/>  <w:LsdException Locked="false" Priority="33" SemiHidden="false"    UnhideWhenUsed="false" QFormat="true" Name="Book Title"/>  <w:LsdException Locked="false" Priority="37" Name="Bibliography"/>  <w:LsdException Locked="false" Priority="39" QFormat="true" Name="TOC Heading"/> </w:LatentStyles></xml><![endif]--><!--[if gte mso 10]><style> /* Style Definitions */  table.MsoNormalTable  {mso-style-name:"Tabla normal";  mso-tstyle-rowband-size:0;  mso-tstyle-colband-size:0;  mso-style-noshow:yes;  mso-style-priority:99;  mso-style-qformat:yes;  mso-style-parent:"";  mso-padding-alt:0cm 5.4pt 0cm 5.4pt;  mso-para-margin:0cm;  mso-para-margin-bottom:.0001pt;  mso-pagination:widow-orphan;  font-size:11.0pt;  font-family:"Calibri","sans-serif";  mso-ascii-font-family:Calibri;  mso-ascii-theme-font:minor-latin;  mso-fareast-font-family:"Times New Roman";  mso-fareast-theme-font:minor-fareast;  mso-hansi-font-family:Calibri;  mso-hansi-theme-font:minor-latin;  mso-bidi-font-family:"Times New Roman";  mso-bidi-theme-font:minor-bidi;} </style><![endif]--> <br /><div class="separator" style="clear: both; text-align: center;"><a href="https://3.bp.blogspot.com/-9tpA5m48Xik/VGM3nfLWZ7I/AAAAAAAAGlU/lvqAQL60mio/s1600/betoniyo_juliste.jpg" imageanchor="1" style="margin-left: 1em; margin-right: 1em;"><img border="0" height="400" src="https://3.bp.blogspot.com/-9tpA5m48Xik/VGM3nfLWZ7I/AAAAAAAAGlU/lvqAQL60mio/s400/betoniyo_juliste.jpg" width="285" /></a></div><div class="MsoNormal" style="text-align: justify;"><br /></div><div class="MsoNormal" style="text-align: justify;"><span style="background-color: black;"><span style="color: white;"><span style="font-family: &quot;Arial&quot;,&quot;sans-serif&quot;; mso-bidi-font-size: 11.0pt;">Simo es un adolescente proveniente de una familia desestructurada, tiene absoluta admiración por su hermano Ilkka, a pesar de que este pasa de él; Ilkka debe ingresar en prisión por una asunto relacionado con el tráfico de estupefacientes, su madre está preocupada y no quiere que su hijo pase su última noche en libertad solo, así que encarga a Simo la labor de pasar junto Ilkka su última noche en libertad.</span></span></span></div><span style="background-color: black;"><span style="color: white;"> </span></span><div class="MsoNormal" style="text-align: justify;"><br /></div><div class="separator" style="clear: both; text-align: center;"><a href="https://3.bp.blogspot.com/-GYOrAo1r_NM/VGM3wTJ3mGI/AAAAAAAAGlc/fDNerltwdgU/s1600/cn_info1440.jpg" imageanchor="1" style="margin-left: 1em; margin-right: 1em;"><img border="0" height="200" src="https://3.bp.blogspot.com/-GYOrAo1r_NM/VGM3wTJ3mGI/AAAAAAAAGlc/fDNerltwdgU/s320/cn_info1440.jpg" width="320" /></a></div><div class="MsoNormal" style="text-align: justify;"><br /></div><span style="background-color: black;"><span style="color: white;"> </span></span><div class="MsoNormal" style="text-align: justify;"><span style="background-color: black;"><span style="color: white;"><span style="font-family: &quot;Arial&quot;,&quot;sans-serif&quot;; mso-bidi-font-size: 11.0pt;">La directora de esta película es Pirjo Honkasalo, responsable de: </span><i style="mso-bidi-font-style: normal;"><span style="font-family: &quot;Arial&quot;,&quot;sans-serif&quot;;">Kainuu 39, Da Capo, 250 Grammaa-Radioaktiivinen Testamentti</span></i><span style="font-family: &quot;Arial&quot;,&quot;sans-serif&quot;;">y <i style="mso-bidi-font-style: normal;">Tulennielijä</i>; </span><span style="font-family: &quot;Arial&quot;,&quot;sans-serif&quot;; mso-bidi-font-size: 11.0pt;">sinceramente me ha sorprendido que sea una directora quien esté al mando de la película, ya os diré luego por qué.</span></span></span></div><span style="background-color: black;"><span style="color: white;"> </span></span><div class="MsoNormal" style="text-align: justify;"><br /></div><div class="separator" style="clear: both; text-align: center;"><a href="https://3.bp.blogspot.com/-3AOqh7R4HQQ/VGM32S12HaI/AAAAAAAAGlk/YJr0IeVZS3s/s1600/Betoniyo_800f.jpg" imageanchor="1" style="margin-left: 1em; margin-right: 1em;"><img border="0" height="180" src="https://3.bp.blogspot.com/-3AOqh7R4HQQ/VGM32S12HaI/AAAAAAAAGlk/YJr0IeVZS3s/s320/Betoniyo_800f.jpg" width="320" /></a></div><div class="MsoNormal" style="text-align: justify;"><br /></div><span style="background-color: black;"><span style="color: white;"> </span></span><div class="MsoNormal" style="text-align: justify;"><span style="background-color: black;"><span style="color: white;"><span style="font-family: &quot;Arial&quot;,&quot;sans-serif&quot;; mso-bidi-font-size: 11.0pt;">Entre los actores señalaremos a: Jari Vilman (<i style="mso-bidi-font-style: normal;">Joki, Kulman Pojat, </i></span><i style="mso-bidi-font-style: normal;"><span style="font-family: &quot;Arial&quot;,&quot;sans-serif&quot;;">Miesten Välisiä Keskusteluja</span></i><span style="font-family: &quot;Arial&quot;,&quot;sans-serif&quot;;">), Jufan Ufsak (<i style="mso-bidi-font-style: normal;">Doktor Stockmann, Röövlirahnu Martin, Detsembrikuumus</i></span><span style="font-family: &quot;Arial&quot;,&quot;sans-serif&quot;; mso-bidi-font-size: 11.0pt;">), Pauli Poraren (</span><i style="mso-bidi-font-style: normal;"><span style="font-family: &quot;Arial&quot;,&quot;sans-serif&quot;;">Tuntematon Sotilas</span></i><i style="mso-bidi-font-style: normal;"><span style="font-family: &quot;Arial&quot;,&quot;sans-serif&quot;; mso-bidi-font-size: 11.0pt;">, Mestari, Armon Aika</span></i><span style="font-family: &quot;Arial&quot;,&quot;sans-serif&quot;; mso-bidi-font-size: 11.0pt;">).</span></span></span></div><span style="background-color: black;"><span style="color: white;"> </span></span><div class="MsoNormal" style="text-align: justify;"><br /></div><div class="separator" style="clear: both; text-align: center;"><a href="https://4.bp.blogspot.com/-uVtmwRXfjbo/VGM370y3inI/AAAAAAAAGls/URwr6KenvOM/s1600/Betoniyo_800h.jpg" imageanchor="1" style="margin-left: 1em; margin-right: 1em;"><img border="0" height="180" src="https://4.bp.blogspot.com/-uVtmwRXfjbo/VGM370y3inI/AAAAAAAAGls/URwr6KenvOM/s320/Betoniyo_800h.jpg" width="320" /></a></div><div class="MsoNormal" style="text-align: justify;"><br /></div><span style="background-color: black;"><span style="color: white;"> </span></span><div class="MsoNormal" style="text-align: justify;"><span style="background-color: black;"><span style="color: white;"><span style="font-family: &quot;Arial&quot;,&quot;sans-serif&quot;; mso-bidi-font-size: 11.0pt;">Esta película está rodada en blanco y negro, es por ello que los contrastes de luz y sombras están muy definidos y bien marcados, también destaca el carácter casi onírico de algunas partes, a<span style="mso-spacerun: yes;">&nbsp; </span>mi parecer esta cinta es hija del cine de Kaneto Shindô (<i style="mso-bidi-font-style: normal;">Onibaba, La Isla Desnuda</i>) y de la más reciente Sin City de la que toma los profundos claroscuros tipo cómic que solo una edición en digital puede lograr. Y, en fin, este es el máximo acierto de la película porque el resto es para darle de comer aparte, o mejor, no darle de comer.</span></span></span></div><span style="background-color: black;"><span style="color: white;"> </span></span><div class="MsoNormal" style="text-align: justify;"><br /></div><div class="separator" style="clear: both; text-align: center;"><a href="https://4.bp.blogspot.com/-6Kmh6wHNDFA/VGM4ByooiXI/AAAAAAAAGl0/C5o4KJRIbOg/s1600/betoniy%C3%B6.jpg" imageanchor="1" style="margin-left: 1em; margin-right: 1em;"><img border="0" height="178" src="https://4.bp.blogspot.com/-6Kmh6wHNDFA/VGM4ByooiXI/AAAAAAAAGl0/C5o4KJRIbOg/s320/betoniy%C3%B6.jpg" width="320" /></a></div><div class="MsoNormal" style="text-align: justify;"><br /></div><span style="background-color: black;"><span style="color: white;"> </span></span><div class="MsoNormal" style="text-align: justify;"><span style="background-color: black;"><span style="color: white;"><span style="font-family: &quot;Arial&quot;,&quot;sans-serif&quot;; mso-bidi-font-size: 11.0pt;">Sí, porque la historia hace aguas por todas partes, no llega a engancharte y a veces no tienes ni pajolera idea de porqué pasan algunas cosas; las actuaciones no destacan por nada, son planas y punto; el ritmo es cansino y abusa de unos primeros planos que en realidad no transmiten nada de nada; también aporta una gran cantidad de filosofía barata (todos los personajes tienen algo que opinar sobre la vida y a algunos se les entiende lo mismo que a Kant) y, al fin, descubriremos que si hay un holocausto nuclear solo sobrevivirían los escorpiones, que yo pensaba que también las cucarachas lo harían pero puedo estar equivocado?</span></span></span></div><span style="background-color: black;"><span style="color: white;"> </span></span><div class="MsoNormal" style="text-align: justify;"><br /></div><div class="separator" style="clear: both; text-align: center;"><a href="https://4.bp.blogspot.com/-W5kbEOhZnz4/VGM4IXRdNEI/AAAAAAAAGl8/yLUfuHFS2Wg/s1600/Steinsteypun%C3%B3tt1.jpg" imageanchor="1" style="margin-left: 1em; margin-right: 1em;"><img border="0" height="180" src="https://4.bp.blogspot.com/-W5kbEOhZnz4/VGM4IXRdNEI/AAAAAAAAGl8/yLUfuHFS2Wg/s320/Steinsteypun%C3%B3tt1.jpg" width="320" /></a></div><div class="MsoNormal" style="text-align: justify;"><br /></div><span style="background-color: black;"><span style="color: white;"> </span></span><div class="MsoNormal" style="text-align: justify;"><span style="background-color: black;"><span style="color: white;"><span style="font-family: &quot;Arial&quot;,&quot;sans-serif&quot;; mso-bidi-font-size: 11.0pt;">Me ha sorprendido en la película que aparecen muy pocas mujeres y que su papel es, como decirlo, nada positivo: la madre de Simo e Ilkka es una borracha que solo piensa en salir de juerga y beber hasta caer rendida descuidando, por supuesto, el bienestar de sus hijos; la otra mujer que aparece es Veera, la novia de Ilkka que no espera ni un día que su novio esté en prisión para ponerle los cuernos, y que se somete a cualquier humillación que le pidan; hablando de humillaciones también se habla del maltrato a la mujer como algo normal y razonable, no sé, quizá haya una crítica subyacente a todo esto pero yo no se la veo, la verdad.</span></span></span></div><span style="background-color: black;"><span style="color: white;"> </span></span><div class="MsoNormal" style="text-align: justify;"><br /></div><div class="separator" style="clear: both; text-align: center;"><a href="https://3.bp.blogspot.com/-Wy_BBySKoCE/VGM4OM8NoUI/AAAAAAAAGmE/OIVw4tggHjk/s1600/Betoniyo_800g.jpg" imageanchor="1" style="margin-left: 1em; margin-right: 1em;"><img border="0" height="208" src="https://3.bp.blogspot.com/-Wy_BBySKoCE/VGM4OM8NoUI/AAAAAAAAGmE/OIVw4tggHjk/s320/Betoniyo_800g.jpg" width="320" /></a></div><div class="MsoNormal" style="text-align: justify;"><br /></div><span style="background-color: black;"><span style="color: white;"> </span></span><div class="MsoNormal" style="text-align: justify;"><span style="color: yellow;"><span style="background-color: black;"><span style="font-family: &quot;Arial&quot;,&quot;sans-serif&quot;;">XI FESTIVAL DE CINE EUROPEO: BETONIYÖ (CONCRETE NIGHT)</span></span></span></div><span style="background-color: black;"><span style="color: white;"> </span></span><div class="MsoNormal" style="background-attachment: scroll; background-clip: border-box; background-image: none; background-origin: padding-box; background-position: 0% 0%; background-repeat: repeat; background-size: auto auto; text-align: justify;"><br /></div><span style="background-color: black;"><span style="color: white;"> </span></span><div class="MsoNormal" style="background-attachment: scroll; background-clip: border-box; background-image: none; background-origin: padding-box; background-position: 0% 0%; background-repeat: repeat; background-size: auto auto; text-align: justify;"><span style="background-color: black;"><span style="color: white;"><span style="font-family: &quot;Arial&quot;,&quot;sans-serif&quot;; mso-bidi-font-size: 11.0pt;">Lo Mejor: Que he descubierto la forma que tienen en Finlandia de encasquetarte la revista <i style="mso-bidi-font-style: normal;">Atalaya</i> (allí también la hay y se llama igual), el sistema es simple, en el metro o en el autobús una chica hermosa se te queda mirando, cuando la miras ella te sonríe y cuando te acercas a ver si pillas te pregunta que piensas acerca de Dios, te come la olla y te endilga la revista (muy inteligente pardiéz, muy inteligente?)</span></span></span></div><span style="background-color: black;"><span style="color: white;"> </span></span><div class="MsoNormal" style="background-attachment: scroll; background-clip: border-box; background-image: none; background-origin: padding-box; background-position: 0% 0%; background-repeat: repeat; background-size: auto auto; text-align: justify;"><span style="background-color: black;"><span style="color: white;"><span style="font-family: &quot;Arial&quot;,&quot;sans-serif&quot;; mso-bidi-font-size: 11.0pt;">Lo Peor: Que a pesar de algunos detalles es muy aburrida.</span></span></span></div><span style="background-color: black;"><span style="color: white;"> </span></span><div class="MsoNormal" style="background-attachment: scroll; background-clip: border-box; background-image: none; background-origin: padding-box; background-position: 0% 0%; background-repeat: repeat; background-size: auto auto; text-align: justify;"><br /></div><span style="background-color: black;"><span style="color: white;"> </span></span><div class="separator" style="clear: both; text-align: center;"><a href="https://3.bp.blogspot.com/-CxaneygGcwY/VGM4r56sHMI/AAAAAAAAGmM/tIp1M32-ejk/s1600/black_and_white_dice.jpg" imageanchor="1" style="clear: left; float: left; margin-bottom: 1em; margin-right: 1em;"><img border="0" height="112" src="https://3.bp.blogspot.com/-CxaneygGcwY/VGM4r56sHMI/AAAAAAAAGmM/tIp1M32-ejk/s200/black_and_white_dice.jpg" width="200" /></a></div><div class="MsoNormal" style="background-attachment: scroll; background-clip: border-box; background-image: none; background-origin: padding-box; background-position: 0% 0%; background-repeat: repeat; background-size: auto auto; text-align: justify;"><span style="background-color: black;"><span style="color: white;"><span style="font-family: &quot;Arial&quot;,&quot;sans-serif&quot;; mso-bidi-font-size: 11.0pt;">Puntuación: 1,5</span></span></span></div><span style="background-color: black;"><span style="color: white;"> <span style="font-family: &quot;Arial&quot;,&quot;sans-serif&quot;; font-size: 12.0pt; mso-ansi-language: ES; mso-bidi-font-size: 11.0pt; mso-bidi-language: AR-SA; mso-fareast-font-family: &quot;Times New Roman&quot;; mso-fareast-language: ES;">El Buen Doctor Afirma: Mal empezamos el día, lo que no sabía es que estaba a punto de empeorar?</span></span></span><br /><br /><br /><!--[if gte mso 9]><xml> <w:LatentStyles DefLockedState="false" DefUnhideWhenUsed="true"   DefSemiHidden="true" DefQFormat="false" DefPriority="99"   LatentStyleCount="267">  <w:LsdException Locked="false" Priority="0" SemiHidden="false"    UnhideWhenUsed="false" QFormat="true" Name="Normal"/>  <w:LsdException Locked="false" Priority="9" SemiHidden="false"    UnhideWhenUsed="false" QFormat="true" Name="heading 1"/>  <w:LsdException Locked="false" Priority="9" QFormat="true" Name="heading 2"/>  <w:LsdException Locked="false" Priority="9" QFormat="true" Name="heading 3"/>  <w:LsdException Locked="false" Priority="9" QFormat="true" Name="heading 4"/>  <w:LsdException Locked="false" Priority="9" QFormat="true" Name="heading 5"/>  <w:LsdException Locked="false" Priority="9" QFormat="true" Name="heading 6"/>  <w:LsdException Locked="false" Priority="9" QFormat="true" Name="heading 7"/>  <w:LsdException Locked="false" Priority="9" QFormat="true" Name="heading 8"/>  <w:LsdException Locked="false" Priority="9" QFormat="true" Name="heading 9"/>  <w:LsdException Locked="false" Priority="39" Name="toc 1"/>  <w:LsdException Locked="false" Priority="39" Name="toc 2"/>  <w:LsdException Locked="false" Priority="39" Name="toc 3"/>  <w:LsdException Locked="false" Priority="39" Name="toc 4"/>  <w:LsdException Locked="false" Priority="39" Name="toc 5"/>  <w:LsdException Locked="false" Priority="39" Name="toc 6"/>  <w:LsdException Locked="false" Priority="39" Name="toc 7"/>  <w:LsdException Locked="false" Priority="39" Name="toc 8"/>  <w:LsdException Locked="fal