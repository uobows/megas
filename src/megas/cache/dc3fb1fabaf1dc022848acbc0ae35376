alse"    UnhideWhenUsed="false" Name="Colorful Grid Accent 5"/>  <w:LsdException Locked="false" Priority="60" SemiHidden="false"    UnhideWhenUsed="false" Name="Light Shading Accent 6"/>  <w:LsdException Locked="false" Priority="61" SemiHidden="false"    UnhideWhenUsed="false" Name="Light List Accent 6"/>  <w:LsdException Locked="false" Priority="62" SemiHidden="false"    UnhideWhenUsed="false" Name="Light Grid Accent 6"/>  <w:LsdException Locked="false" Priority="63" SemiHidden="false"    UnhideWhenUsed="false" Name="Medium Shading 1 Accent 6"/>  <w:LsdException Locked="false" Priority="64" SemiHidden="false"    UnhideWhenUsed="false" Name="Medium Shading 2 Accent 6"/>  <w:LsdException Locked="false" Priority="65" SemiHidden="false"    UnhideWhenUsed="false" Name="Medium List 1 Accent 6"/>  <w:LsdException Locked="false" Priority="66" SemiHidden="false"    UnhideWhenUsed="false" Name="Medium List 2 Accent 6"/>  <w:LsdException Locked="false" Priority="67" SemiHidden="false"    UnhideWhenUsed="false" Name="Medium Grid 1 Accent 6"/>  <w:LsdException Locked="false" Priority="68" SemiHidden="false"    UnhideWhenUsed="false" Name="Medium Grid 2 Accent 6"/>  <w:LsdException Locked="false" Priority="69" SemiHidden="false"    UnhideWhenUsed="false" Name="Medium Grid 3 Accent 6"/>  <w:LsdException Locked="false" Priority="70" SemiHidden="false"    UnhideWhenUsed="false" Name="Dark List Accent 6"/>  <w:LsdException Locked="false" Priority="71" SemiHidden="false"    UnhideWhenUsed="false" Name="Colorful Shading Accent 6"/>  <w:LsdException Locked="false" Priority="72" SemiHidden="false"    UnhideWhenUsed="false" Name="Colorful List Accent 6"/>  <w:LsdException Locked="false" Priority="73" SemiHidden="false"    UnhideWhenUsed="false" Name="Colorful Grid Accent 6"/>  <w:LsdException Locked="false" Priority="19" SemiHidden="false"    UnhideWhenUsed="false" QFormat="true" Name="Subtle Emphasis"/>  <w:LsdException Locked="false" Priority="21" SemiHidden="false"    UnhideWhenUsed="false" QFormat="true" Name="Intense Emphasis"/>  <w:LsdException Locked="false" Priority="31" SemiHidden="false"    UnhideWhenUsed="false" QFormat="true" Name="Subtle Reference"/>  <w:LsdException Locked="false" Priority="32" SemiHidden="false"    UnhideWhenUsed="false" QFormat="true" Name="Intense Reference"/>  <w:LsdException Locked="false" Priority="33" SemiHidden="false"    UnhideWhenUsed="false" QFormat="true" Name="Book Title"/>  <w:LsdException Locked="false" Priority="37" Name="Bibliography"/>  <w:LsdException Locked="false" Priority="39" QFormat="true" Name="TOC Heading"/> </w:LatentStyles></xml><![endif]--><!--[if gte mso 10]><style> /* Style Definitions */  table.MsoNormalTable  {mso-style-name:"Tabla normal";  mso-tstyle-rowband-size:0;  mso-tstyle-colband-size:0;  mso-style-noshow:yes;  mso-style-priority:99;  mso-style-qformat:yes;  mso-style-parent:"";  mso-padding-alt:0cm 5.4pt 0cm 5.4pt;  mso-para-margin:0cm;  mso-para-margin-bottom:.0001pt;  mso-pagination:widow-orphan;  font-size:11.0pt;  font-family:"Calibri","sans-serif";  mso-ascii-font-family:Calibri;  mso-ascii-theme-font:minor-latin;  mso-fareast-font-family:"Times New Roman";  mso-fareast-theme-font:minor-fareast;  mso-hansi-font-family:Calibri;  mso-hansi-theme-font:minor-latin;  mso-bidi-font-family:"Times New Roman";  mso-bidi-theme-font:minor-bidi;} </style><![endif]--> <br /><div class="separator" style="clear: both; text-align: center;"><a href="http://3.bp.blogspot.com/-vmeXIN-SCsQ/VGt06Q5VmRI/AAAAAAAAGuM/DCFYxiWvRec/s1600/maxresdefault.jpg" imageanchor="1" style="margin-left: 1em; margin-right: 1em;"><img border="0" src="http://3.bp.blogspot.com/-vmeXIN-SCsQ/VGt06Q5VmRI/AAAAAAAAGuM/DCFYxiWvRec/s1600/maxresdefault.jpg" height="400" width="280" /></a></div><div class="MsoNormal" style="text-align: justify;"><br /></div><div class="MsoNormal" style="text-align: justify;"><span style="background-color: black;"><span style="color: white;"><span style="font-family: &quot;Arial&quot;,&quot;sans-serif&quot;; mso-bidi-font-size: 11.0pt;">Existe en Sevilla, concretamente en el barrio de Rochelambert, un centro donde se enseña a boxear, entre sus asistentes hay de todo, desde personas del más alto nivel socioeconómico hasta ex convictos, ¿Cómo es posible que una mezcla de ese calibre no estalle? Gracias al responsable y entrenador del centro, una gran persona llamada el Bigotes</span></span></span></div><span style="background-color: black;"><span style="color: white;"> </span></span><div class="MsoNormal" style="text-align: justify;"><br /></div><div class="separator" style="clear: both; text-align: center;"><a href="http://2.bp.blogspot.com/-L5_m836PIyM/VGt1CYQIH8I/AAAAAAAAGuU/zV9PmvQcEqQ/s1600/320-bigotes.jpg" imageanchor="1" style="margin-left: 1em; margin-right: 1em;"><img border="0" src="http://2.bp.blogspot.com/-L5_m836PIyM/VGt1CYQIH8I/AAAAAAAAGuU/zV9PmvQcEqQ/s1600/320-bigotes.jpg" /></a></div><div class="MsoNormal" style="text-align: justify;"><br /></div><span style="background-color: black;"><span style="color: white;"> </span></span><div class="MsoNormal" style="text-align: justify;"><span style="background-color: black;"><span style="color: white;"><span style="font-family: &quot;Arial&quot;,&quot;sans-serif&quot;; mso-bidi-font-size: 11.0pt;">Este documental está dirigido por Álvaro Torrellas, que solo ha dirigido/ guionizado/ producido) este documental y? ha sido jefe de producción de un episodio de la serie de Bob Esponja? no sé, me parece raro?</span></span></span></div><span style="background-color: black;"><span style="color: white;"> </span></span><div class="MsoNormal" style="text-align: justify;"><br /></div><div class="separator" style="clear: both; text-align: center;"><a href="http://2.bp.blogspot.com/-O3KLGn1E8yk/VGt1IFezmhI/AAAAAAAAGuc/Zq7jIe9e0qo/s1600/0_p2xmdtio.jpg" imageanchor="1" style="margin-left: 1em; margin-right: 1em;"><img border="0" src="http://2.bp.blogspot.com/-O3KLGn1E8yk/VGt1IFezmhI/AAAAAAAAGuc/Zq7jIe9e0qo/s1600/0_p2xmdtio.jpg" height="200" width="320" /></a></div><div class="MsoNormal" style="text-align: justify;"><br /></div><span style="background-color: black;"><span style="color: white;"> </span></span><div class="MsoNormal" style="text-align: justify;"><span style="background-color: black;"><span style="color: white;"><span style="font-family: &quot;Arial&quot;,&quot;sans-serif&quot;; mso-bidi-font-size: 11.0pt;">Este documental pivota entre la vida de varios personajes: el propio Bigotes, él reconoce que no tiene estudios pero sabiduría de la vida tiene para parar ocho barcos, como él dice nada más ver entrar a una persona sabe de que palo va y por eso sabe como tratar a cada uno en cada momento; su mejor arma es el humor, un humor contagioso que seguro te sacará una sonrisa a pesar de cualquier cosa que te este pasando, también se pone al nivel de cualquiera, es por eso que es capaz de tratar con cualquier persona da igual su procedencia y sus defectos, hace una labor psicológica de integración enorme y como él dice a todo el que entra: <i style="mso-bidi-font-style: normal;">Mientras te portes bien las puertas permanecerán abiertas, si la lías o te peleas con alguien te mando al carajo</i>.</span></span></span></div><span style="background-color: black;"><span style="color: white;"> </span></span><div class="MsoNormal" style="text-align: justify;"><br /></div><div class="separator" style="clear: both; text-align: center;"><a href="http://1.bp.blogspot.com/-ynktz6AATA4/VGt1d9qKgKI/AAAAAAAAGus/2rSJej5VwR4/s1600/Round-3.jpg" imageanchor="1" style="margin-left: 1em; margin-right: 1em;"><img border="0" src="http://1.bp.blogspot.com/-ynktz6AATA4/VGt1d9qKgKI/AAAAAAAAGus/2rSJej5VwR4/s1600/Round-3.jpg" height="200" width="320" /></a></div><div class="MsoNormal" style="text-align: justify;"><br /></div><span style="background-color: black;"><span style="color: white;"> </span></span><div class="MsoNormal" style="text-align: justify;"><span style="background-color: black;"><span style="color: white;"><span style="font-family: &quot;Arial&quot;,&quot;sans-serif&quot;; mso-bidi-font-size: 11.0pt;">Otros personajes que aparecen son sus dos hijos (que también regentan gimnasios a la manera del Bigotes y que igual que su padre han sido boxeadores) y tres jóvenes problemáticos: el Mesías (que tras estar internado demasiado tiempo en un psiquiátrico busca reencauzar su vida), el Kaka (un joven que tras aparecer como actor en <i style="mso-bidi-font-style: normal;">7 Vírgenes</i> se dejó cegar por la fama y se convirtió en delincuente) y el Torero (que quedó profundamente traumatizado cuando su prima con la que había convivido desde la infancia fue asesinada por su pareja).</span></span></span></div><span style="background-color: black;"><span style="color: white;"> </span></span><div class="MsoNormal" style="text-align: justify;"><br /></div><div class="separator" style="clear: both; text-align: center;"><a href="http://4.bp.blogspot.com/-_PTUS1nESiM/VGt1lTjSd1I/AAAAAAAAGu0/w6gF0WpOk5g/s1600/Round-2.jpg" imageanchor="1" style="margin-left: 1em; margin-right: 1em;"><img border="0" src="http://4.bp.blogspot.com/-_PTUS1nESiM/VGt1lTjSd1I/AAAAAAAAGu0/w6gF0WpOk5g/s1600/Round-2.jpg" height="200" width="320" /></a></div><div class="MsoNormal" style="text-align: justify;"><br /></div><span style="background-color: black;"><span style="color: white;"> </span></span><div class="MsoNormal" style="text-align: justify;"><span style="background-color: black;"><span style="color: white;"><span style="font-family: &quot;Arial&quot;,&quot;sans-serif&quot;; mso-bidi-font-size: 11.0pt;">Con este documental aprenderemos que muchas veces los problemas que nos agobian no son tan graves, hay que sobrevivir y luchar, no quedarse en casa lamentándose y quejándose por todo a cualquiera que tenga la desgracia de poner la oreja; el deporte (el boxeo en este caso) nos puede ayudar a despejar tensiones, hacer nuevas amistades, olvidarnos durante un rato de las preocupaciones y salir del pozo en que estemos metidos (quien lo esté, claro) y si para eso cuentas con la ayuda del Bigotes tienes gran parte del camino andado.</span></span></span></div><span style="background-color: black;"><span style="color: white;"> </span></span><div class="MsoNormal" style="text-align: justify;"><br /></div><div class="separator" style="clear: both; text-align: center;"><a href="http://3.bp.blogspot.com/-YFZeESy3Stw/VGt1sxSF6KI/AAAAAAAAGu8/kdc8qeBMliU/s1600/3minutosweb_10.jpg" imageanchor="1" style="margin-left: 1em; margin-right: 1em;"><img border="0" src="http://3.bp.blogspot.com/-YFZeESy3Stw/VGt1sxSF6KI/AAAAAAAAGu8/kdc8qeBMliU/s1600/3minutosweb_10.jpg" height="213" width="320" /></a></div><div class="MsoNormal" style="text-align: justify;"><br /></div><span style="background-color: black;"><span style="color: white;"> </span></span><div class="MsoNormal" style="text-align: justify;"><span style="color: yellow;"><span style="background-color: black;"><span style="font-family: &quot;Arial&quot;,&quot;sans-serif&quot;;">XI FESTIVAL DE CINE EUROPEO: 3 MINUTOS. SI LUCHAS NO HAY DERROTA</span></span></span></div><span style="background-color: black;"><span style="color: white;"> </span></span><div class="MsoNormal" style="background-attachment: scroll; background-clip: border-box; background-image: none; background-origin: padding-box; background-position: 0% 0%; background-repeat: repeat; background-size: auto auto; text-align: justify;"><br /></div><span style="background-color: black;"><span style="color: white;"> </span></span><div class="MsoNormal" style="background-attachment: scroll; background-clip: border-box; background-image: none; background-origin: padding-box; background-position: 0% 0%; background-repeat: repeat; background-size: auto auto; text-align: justify;"><span style="background-color: black;"><span style="color: white;"><span style="font-family: &quot;Arial&quot;,&quot;sans-serif&quot;; mso-bidi-font-size: 11.0pt;">Lo Mejor: El Bigotes, por ser una gran persona y un gran personaje.</span></span></span></div><span style="background-color: black;"><span style="color: white;"> </span></span><div class="MsoNormal" style="background-attachment: scroll; background-clip: border-box; background-image: none; background-origin: padding-box; background-position: 0% 0%; background-repeat: repeat; background-size: auto auto; text-align: justify;"><span style="background-color: black;"><span style="color: white;"><span style="font-family: &quot;Arial&quot;,&quot;sans-serif&quot;; mso-bidi-font-size: 11.0pt;">Lo Peor: Esperemos que a raíz del documental no vayan allí cuatro gilipollas a tocarle las pelotas a tan buena persona.</span></span></span></div><span style="background-color: black;"><span style="color: white;"> </span></span><div class="MsoNormal" style="background-attachment: scroll; background-clip: border-box; background-image: none; background-origin: padding-box; background-position: 0% 0%; background-repeat: repeat; background-size: auto auto; text-align: justify;"><br /></div><span style="background-color: black;"><span style="color: white;"> </span></span><div class="separator" style="clear: both; text-align: center;"><a href="http://4.bp.blogspot.com/-v8yopEEFSh4/VGt16GzqD2I/AAAAAAAAGvE/o4DW4mfVRvQ/s1600/14160864729945.jpg" imageanchor="1" style="clear: left; float: left; margin-bottom: 1em; margin-right: 1em;"><img border="0" src="http://4.bp.blogspot.com/-v8yopEEFSh4/VGt16GzqD2I/AAAAAAAAGvE/o4DW4mfVRvQ/s1600/14160864729945.jpg" height="137" width="200" /></a></div><div class="MsoNormal" style="background-attachment: scroll; background-clip: border-box; background-image: none; background-origin: padding-box; background-position: 0% 0%; background-repeat: repeat; background-size: auto auto; text-align: justify;"><span style="background-color: black;"><span style="color: white;"><span style="font-family: &quot;Arial&quot;,&quot;sans-serif&quot;; mso-bidi-font-size: 11.0pt;">Puntuación: 7</span></span></span></div><span style="background-color: black;"><span style="color: white;"> </span></span><div class="MsoNormal" style="background-attachment: scroll; background-clip: border-box; background-image: none; background-origin: padding-box; background-position: 0% 0%; background-repeat: repeat; background-size: auto auto; text-align: justify;"><span style="background-color: black;"><span style="color: white;"><span style="font-family: &quot;Arial&quot;,&quot;sans-serif&quot;; mso-bidi-font-size: 11.0pt;">El Buen Doctor Afirma: Y sí, lo siguiente que veáis por aquí será una de esas pelis que tanto os gustan, todo gracias al Maratón Grotesco Palomitero, un evento de esos de los de no perderse si eres un fan del cine de autor (de auto cutre se sobreentiende)</span></span></span></div><div class="MsoNormal" style="background-attachment: scroll; background-clip: border-box; background-image: none; background-origin: padding-box; background-position: 0% 0%; background-repeat: repeat; background-size: auto auto; text-align: justify;"><br /></div><br /><div class="MsoNormal" style="background-attachment: scroll; background-clip: border-box; background-image: none; background-origin: padding-box; background-position: 0% 0%; background-repeat: repeat; background-size: auto auto; text-align: justify;"><!--[if gte mso 9]><xml> <w:LatentStyles DefLockedState="false" DefUnhideWhenUsed="true"   DefSemiHidden="true" DefQFormat="false" DefPriority="99"   LatentStyleCount="267">  <w:LsdException Locked="false" Priority="0" SemiHidden="false"    UnhideWhenUsed="false" QFormat="true" Name="Normal"/>  <w:LsdException Locked="false" Priority="9" SemiHidden="false"    UnhideWhenUsed="false" QFormat="true" Name="heading 1"/>  <w:LsdException Locked="false" Priority="9" QFormat="true" Name="heading 2"/>  <w:LsdException Locked="false" Priority="9" QFormat="true" Name="heading 3"/>  <w:LsdException Locked="false" Priority="9" QFormat="true" Name="heading 4"/>  <w:LsdException Locked="