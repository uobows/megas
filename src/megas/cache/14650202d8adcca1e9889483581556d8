ked="false" Priority="31" SemiHidden="false"    UnhideWhenUsed="false" QFormat="true" Name="Subtle Reference"/>  <w:LsdException Locked="false" Priority="32" SemiHidden="false"    UnhideWhenUsed="false" QFormat="true" Name="Intense Reference"/>  <w:LsdException Locked="false" Priority="33" SemiHidden="false"    UnhideWhenUsed="false" QFormat="true" Name="Book Title"/>  <w:LsdException Locked="false" Priority="37" Name="Bibliography"/>  <w:LsdException Locked="false" Priority="39" QFormat="true" Name="TOC Heading"/> </w:LatentStyles></xml><![endif]--><!--[if gte mso 10]><style> /* Style Definitions */  table.MsoNormalTable  {mso-style-name:"Tabla normal";  mso-tstyle-rowband-size:0;  mso-tstyle-colband-size:0;  mso-style-noshow:yes;  mso-style-priority:99;  mso-style-qformat:yes;  mso-style-parent:"";  mso-padding-alt:0cm 5.4pt 0cm 5.4pt;  mso-para-margin-top:0cm;  mso-para-margin-right:0cm;  mso-para-margin-bottom:10.0pt;  mso-para-margin-left:0cm;  line-height:115%;  mso-pagination:widow-orphan;  font-size:11.0pt;  font-family:"Calibri","sans-serif";  mso-ascii-font-family:Calibri;  mso-ascii-theme-font:minor-latin;  mso-fareast-font-family:"Times New Roman";  mso-fareast-theme-font:minor-fareast;  mso-hansi-font-family:Calibri;  mso-hansi-theme-font:minor-latin;} </style><![endif]--><!--[if gte mso 9]><xml> <o:shapedefaults v:ext="edit" spidmax="1026"/></xml><![endif]--><!--[if gte mso 9]><xml> <o:shapelayout v:ext="edit">  <o:idmap v:ext="edit" data="1"/> </o:shapelayout></xml><![endif]--> </div><div align="center" class="MsoNormal" style="margin-bottom: 0.0001pt; text-align: center;"><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;">En la más remota antigüedad</span></b></span></span></div><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;"> </span></b></span></span><div align="center" class="MsoNormal" style="margin-bottom: 0.0001pt; text-align: center;"><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;">las transacciones de la primitiva humanidad</span></b></span></span></div><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;"> </span></b></span></span><div align="center" class="MsoNormal" style="margin-bottom: 0.0001pt; text-align: center;"><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;">se basaban en el intercambio de bienes.</span></b></span></span></div><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;"> </span></b></span></span><div align="center" class="MsoNormal" style="margin-bottom: 0.0001pt; text-align: center;"><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;">Un ejemplo ilustrará lo anterior</span></b></span></span></div><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;"> </span></b></span></span><div align="center" class="MsoNormal" style="margin-bottom: 0.0001pt; text-align: center;"><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;">por lo cual la gente vivía bastante mejor:</span></b></span></span></div><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;"> </span></b></span></span><div align="center" class="MsoNormal" style="margin-bottom: 0.0001pt; text-align: center;"><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;">Te cambio parte de mi trigo por varias ovejas que tú tienes.</span></b></span></span></div><div align="center" class="MsoNormal" style="margin-bottom: 0.0001pt; text-align: center;"><br /></div><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;"> </span></b></span></span><div align="center" class="MsoNormal" style="margin-bottom: 0.0001pt; text-align: center;"><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;">Más tarde los alimentos fueron sustituidos por minerales:</span></b></span></span></div><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;"> </span></b></span></span><div align="center" class="MsoNormal" style="margin-bottom: 0.0001pt; text-align: center;"><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;">Piedras preciosas, oro, plata y otro tipo de metales</span></b></span></span></div><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;"> </span></b></span></span><div align="center" class="MsoNormal" style="margin-bottom: 0.0001pt; text-align: center;"><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;">los cuales fueron los precursores</span></b></span></span></div><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;"> </span></b></span></span><div align="center" class="MsoNormal" style="margin-bottom: 0.0001pt; text-align: center;"><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;">de las monedas ya más posteriores.</span></b></span></span></div><div align="center" class="MsoNormal" style="margin-bottom: 0.0001pt; text-align: center;"><br /></div><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;"> </span></b></span></span><div align="center" class="MsoNormal" style="margin-bottom: 0.0001pt; text-align: center;"><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;">Con la acuñación de la metálica moneda</span></b></span></span></div><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;"> </span></b></span></span><div align="center" class="MsoNormal" style="margin-bottom: 0.0001pt; text-align: center;"><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;">se inauguró el dinero, en una nueva era</span></b></span></span></div><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;"> </span></b></span></span><div align="center" class="MsoNormal" style="margin-bottom: 0.0001pt; text-align: center;"><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;">de ahí el dicho que todavía en la actualidad</span></b></span></span></div><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;"> </span></b></span></span><div align="center" class="MsoNormal" style="margin-bottom: 0.0001pt; text-align: center;"><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;">perdura en una especie de atavismo, de antigüedad:</span></b></span></span></div><div align="center" class="MsoNormal" style="margin-bottom: 0.0001pt; text-align: center;"><br /></div><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;"> </span></b></span></span><div align="center" class="MsoNormal" style="margin-bottom: 0.0001pt; text-align: center;"><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;">A mí me pagas ahora y de aquí en adelante</span></b></span></span></div><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;"> </span></b></span></span><div align="center" class="MsoNormal" style="margin-bottom: 0.0001pt; text-align: center;"><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;">en dinero contante y sonante;</span></b></span></span></div><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;"> </span></b></span></span><div align="center" class="MsoNormal" style="margin-bottom: 0.0001pt; text-align: center;"><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;">ó este otro dicho que es más revelador </span></b></span></span></div><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;"> </span></b></span></span><div align="center" class="MsoNormal" style="margin-bottom: 0.0001pt; text-align: center;"><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;">de la naturaleza de este poderoso caballero</span></b></span></span></div><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;"> </span></b></span></span><div align="center" class="MsoNormal" style="margin-bottom: 0.0001pt; text-align: center;"><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;">que es Don dinero</span></b></span></span></div><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;"> </span></b></span></span><div align="center" class="MsoNormal" style="margin-bottom: 0.0001pt; text-align: center;"><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;">como decía Quevedo</span></b></span></span></div><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;"> </span></b></span></span><div align="center" class="MsoNormal" style="margin-bottom: 0.0001pt; text-align: center;"><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;">¿prefiere un talón o lo abonamos en metálico al señor?</span></b></span></span></div><div align="center" class="MsoNormal" style="margin-bottom: 0.0001pt; text-align: center;"><br /></div><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;"> </span></b></span></span><div align="center" class="MsoNormal" style="margin-bottom: 0.0001pt; text-align: center;"><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;">Las monedas en una bolsa se guardaban</span></b></span></span></div><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;"> </span></b></span></span><div align="center" class="MsoNormal" style="margin-bottom: 0.0001pt; text-align: center;"><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;">de ahí que en otra época, los ladrones</span></b></span></span></div><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;"> </span></b></span></span><div align="center" class="MsoNormal" style="margin-bottom: 0.0001pt; text-align: center;"><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;">cuando a una persona atracaban</span></b></span></span></div><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;"> </span></b></span></span><div align="center" class="MsoNormal" style="margin-bottom: 0.0001pt; text-align: center;"><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;">con esta frase anunciaban sus intenciones:</span></b></span></span></div><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;"> </span></b></span></span><div align="center" class="MsoNormal" style="margin-bottom: 0.0001pt; text-align: center;"><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;">?La bolsa o la vida?, así, sin contemplaciones.</span></b></span></span></div><div align="center" class="MsoNormal" style="margin-bottom: 0.0001pt; text-align: center;"><br /></div><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;"> </span></b></span></span><div align="center" class="MsoNormal" style="margin-bottom: 0.0001pt; text-align: center;"><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;"><span>&nbsp;</span>Más adelante de papel se hicieron los billetes</span></b></span></span></div><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;"> </span></b></span></span><div align="center" class="MsoNormal" style="margin-bottom: 0.0001pt; text-align: center;"><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;">que fácilmente los metes</span></b></span></span></div><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;"> </span></b></span></span><div align="center" class="MsoNormal" style="margin-bottom: 0.0001pt; text-align: center;"><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;">en billeteros más transportables</span></b></span></span></div><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;"> </span></b></span></span><div align="center" class="MsoNormal" style="margin-bottom: 0.0001pt; text-align: center;"><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;">ligeros y confortables</span></b></span></span></div><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;"> </span></b></span></span><div align="center" class="MsoNormal" style="margin-bottom: 0.0001pt; text-align: center;"><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;">y en grandes cantidades</span></b></span></span></div><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;"> </span></b></span></span><div align="center" class="MsoNormal" style="margin-bottom: 0.0001pt; text-align: center;"><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;">el maletín lo traslada a lejanas ciudades.</span></b></span></span></div><div align="center" class="MsoNormal" style="margin-bottom: 0.0001pt; text-align: center;"><br /></div><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;"> </span></b></span></span><div align="center" class="MsoNormal" style="margin-bottom: 0.0001pt; text-align: center;"><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;">Hay una ley que marca la evolución</span></b></span></span></div><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;"> </span></b></span></span><div align="center" class="MsoNormal" style="margin-bottom: 0.0001pt; text-align: center;"><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;">del dinero en cuestión</span></b></span></span></div><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;"> </span></b></span></span><div align="center" class="MsoNormal" style="margin-bottom: 0.0001pt; text-align: center;"><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;">y es que éste tiende a su material desaparición:</span></b></span></span></div><div align="center" class="MsoNormal" style="margin-bottom: 0.0001pt; text-align: center;"><br /></div><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;"> </span></b></span></span><div align="center" class="MsoNormal" style="margin-bottom: 0.0001pt; text-align: center;"><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;">Primero fue el pesado mineral</span></b></span></span></div><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;"> </span></b></span></span><div align="center" class="MsoNormal" style="margin-bottom: 0.0001pt; text-align: center;"><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;">que fue sustituido por la moneda de metal.</span></b></span></span></div><div align="center" class="MsoNormal" style="margin-bottom: 0.0001pt; text-align: center;"><br /></div><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;"> </span></b></span></span><div align="center" class="MsoNormal" style="margin-bottom: 0.0001pt; text-align: center;"><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;">Después el billete de papel entra en acción</span></b></span></span></div><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;"> </span></b></span></span><div align="center" class="MsoNormal" style="margin-bottom: 0.0001pt; text-align: center;"><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;">pero en gran cantidad</span></b></span></span></div><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;"> </span></b></span></span><div align="center" class="MsoNormal" style="margin-bottom: 0.0001pt; text-align: center;"><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;">los billetes y monedas abultan hasta la incomodidad</span></b></span></span></div><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;"> </span></b></span></span><div align="center" class="MsoNormal" style="margin-bottom: 0.0001pt; text-align: center;"><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;">y entonces la tarjeta hace su aparición:</span></b></span></span></div><div align="center" class="MsoNormal" style="margin-bottom: 0.0001pt; text-align: center;"><br /></div><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;"> </span></b></span></span><div align="center" class="MsoNormal" style="margin-bottom: 0.0001pt; text-align: center;"><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;">Se introduce en un aparato del comercio y el dependiente</span></b></span></span></div><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;"> </span></b></span></span><div align="center" class="MsoNormal" style="margin-bottom: 0.0001pt; text-align: center;"><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;">teclea la cantidad</span></b></span></span></div><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;"> </span></b></span></span><div align="center" class="MsoNormal" style="margin-bottom: 0.0001pt; text-align: center;"><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;">y compras con toda facilidad</span></b></span></span></div><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;"> </span></b></span></span><div align="center" class="MsoNormal" style="margin-bottom: 0.0001pt; text-align: center;"><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;">siempre y cuando tenga liquidez tu cuenta corriente</span></b></span></span></div><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;"> </span></b></span></span><div align="center" class="MsoNormal" style="margin-bottom: 0.0001pt; text-align: center;"><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;">que se encuentra presente</span></b></span></span></div><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;"> </span></b></span></span><div align="center" class="MsoNormal" style="margin-bottom: 0.0001pt; text-align: center;"><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;">en el banco donde eres cliente.</span></b></span></span></div><div align="center" class="MsoNormal" style="margin-bottom: 0.0001pt; text-align: center;"><br /></div><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;"> </span></b></span></span><div align="center" class="MsoNormal" style="margin-bottom: 0.0001pt; text-align: center;"><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;">Así que el dinero actualmente</span></b></span></span></div><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;"> </span></b></span></span><div align="center" class="MsoNormal" style="margin-bottom: 0.0001pt; text-align: center;"><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;">se reduce a mera numeración</span></b></span></span></div><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;"> </span></b></span></span><div align="center" class="MsoNormal" style="margin-bottom: 0.0001pt; text-align: center;"><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;">y con una tarjeta de plástico tienes la solución.</span></b></span></span></div><div align="center" class="MsoNormal" style="margin-bottom: 0.0001pt; text-align: center;"><br /></div><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;"> </span></b></span></span><div align="center" class="MsoNormal" style="margin-bottom: 0.0001pt; text-align: center;"><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;">Y el próximo escalón</span></b></span></span></div><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;"> </span></b></span></span><div align="center" class="MsoNormal" style="margin-bottom: 0.0001pt; text-align: center;"><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;">será el versátil móvil multifunción</span></b></span></span></div><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;"> </span></b></span></span><div align="center" class="MsoNormal" style="margin-bottom: 0.0001pt; text-align: center;"><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;"><span>&nbsp;</span>y más adelante los "chips" en el cerebro implantados</span></b></span></span></div><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;"> </span></b></span></span><div align="center" class="MsoNormal" style="margin-bottom: 0.0001pt; text-align: center;"><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;">nos tendrán con nuestro banco conectados</span></b></span></span></div><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;"> </span></b></span></span><div align="center" class="MsoNormal" style="margin-bottom: 0.0001pt; text-align: center;"><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;">y bastará un escáner de identificación</span></b></span></span></div><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;"> </span></b></span></span><div align="center" class="MsoNormal" style="margin-bottom: 0.0001pt; text-align: center;"><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;">para cualquier monetaria acción.</span></b></span></span></div><div align="center" class="MsoNormal" style="margin-bottom: 0.0001pt; text-align: center;"><br /></div><div class="separator" style="clear: both; text-align: center;"><a href="http://3.bp.blogspot.com/-BrX194WchEc/VNEQLkXZXkI/AAAAAAAAC44/Acowulbe49I/s1600/creditoperu.jpg" imageanchor="1" style="margin-left: 1em; margin-right: 1em;"><img border="0" src="http://3.bp.blogspot.com/-BrX194WchEc/VNEQLkXZXkI/AAAAAAAAC44/Acowulbe49I/s1600/creditoperu.jpg" height="300" width="320" /></a></div><div align="center" class="MsoNormal" style="margin-bottom: 0.0001pt; text-align: center;"><br /></div>";s:4:"link";s:52:"http://parapoeta.blogspot.com/2015/02/el-dinero.html";s:6:"author";s:63:"noreply@blogger.com (musicoclasico parapoeta real y fantástico)";s:7:"summary";s:46614:"<div style="text-align: center;"><br /></div><div style="text-align: center;"><br /></div><div class="separator" style="clear: both; text-align: center;"><a href="http://1.bp.blogspot.com/-qOifiOWQjTo/VNEQGtETqvI/AAAAAAAAC4w/2PUcooyDefc/s1600/611x458.jpg" imageanchor="1" style="margin-left: 1em; margin-right: 1em;"><img b