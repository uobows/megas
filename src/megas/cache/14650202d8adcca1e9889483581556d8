is"/>  <w:LsdException Locked="false" Priority="21" SemiHidden="false"    UnhideWhenUsed="false" QFormat="true" Name="Intense Emphasis"/>  <w:LsdException Locked="false" Priority="31" SemiHidden="false"    UnhideWhenUsed="false" QFormat="true" Name="Subtle Reference"/>  <w:LsdException Locked="false" Priority="32" SemiHidden="false"    UnhideWhenUsed="false" QFormat="true" Name="Intense Reference"/>  <w:LsdException Locked="false" Priority="33" SemiHidden="false"    UnhideWhenUsed="false" QFormat="true" Name="Book Title"/>  <w:LsdException Locked="false" Priority="37" Name="Bibliography"/>  <w:LsdException Locked="false" Priority="39" QFormat="true" Name="TOC Heading"/> </w:LatentStyles></xml><![endif]-->   </div><div align="center" class="MsoNormal" style="text-align: center;"><br /></div><div align="center" class="MsoNormal" style="text-align: center;"><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;">Desde que existe la humanidad</span></b></span></span></div><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;"> </span></b></span></span><div align="center" class="MsoNormal" style="text-align: center;"><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;">ha sido controlada por sus dirigentes</span></b></span></span></div><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;"> </span></b></span></span><div align="center" class="MsoNormal" style="text-align: center;"><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;">que han manejado a las masas con más o menos habilidad</span></b></span></span></div><span style="font-fam