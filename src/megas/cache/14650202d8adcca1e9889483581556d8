Name="Dark List Accent 6"/>  <w:LsdException Locked="false" Priority="71" SemiHidden="false"    UnhideWhenUsed="false" Name="Colorful Shading Accent 6"/>  <w:LsdException Locked="false" Priority="72" SemiHidden="false"    UnhideWhenUsed="false" Name="Colorful List Accent 6"/>  <w:LsdException Locked="false" Priority="73" SemiHidden="false"    UnhideWhenUsed="false" Name="Colorful Grid Accent 6"/>  <w:LsdException Locked="false" Priority="19" SemiHidden="false"    UnhideWhenUsed="false" QFormat="true" Name="Subtle Emphasis"/>  <w:LsdException Locked="false" Priority="21" SemiHidden="false"    UnhideWhenUsed="false" QFormat="true" Name="Intense Emphasis"/>  <w:LsdException Locked="false" Priority="31" SemiHidden="false"    UnhideWhenUsed="false" QFormat="true" Name="Subtle Reference"/>  <w:LsdException Locked="false" Priority="32" SemiHidden="false"    UnhideWhenUsed="false" QFormat="true" Name="Intense Reference"/>  <w:LsdException Locked="false" Priority="33" SemiHidden="false"    UnhideWhenUsed="false" QFormat="true" Name="Book Title"/>  <w:LsdException Locked="false" Priority="37" Name="Bibliography"/>  <w:LsdException Locked="false" Priority="39" QFormat="true" Name="TOC Heading"/> </w:LatentStyles></xml><![endif]--><!--[if gte mso 10]><style> /* Style Definitions */  table.MsoNormalTable  {mso-style-name:"Tabla normal";  mso-tstyle-rowband-size:0;  mso-tstyle-colband-size:0;  mso-style-noshow:yes;  mso-style-priority:99;  mso-style-qformat:yes;  mso-style-parent:"";  mso-padding-alt:0cm 5.4pt 0cm 5.4pt;  mso-para-margin-top:0cm;  mso-para-margin-right:0cm;  mso-para-margin-bottom:10.0pt;  mso-para-margin-left:0cm;  line-height:115%;  mso-pagination:widow-orphan;  font-size:11.0pt;  font-family:"Calibri","sans-serif";  mso-ascii-font-family:Calibri;  mso-ascii-theme-font:minor-latin;  mso-fareast-font-family:"Times New Roman";  mso-fareast-theme-font:minor-fareast;  mso-hansi-font-family:Calibri;  mso-hansi-theme-font:minor-latin;} </style><![endif]--> <br /><div align="center" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: center;"><b><span style="color: red; font-family: &quot;Arial&quot;,&quot;sans-serif&quot;; font-size: 18.0pt;">Me refiero a las muy utilizadas </span></b></div><div align="center" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: center;"><b><span style="color: red; font-family: &quot;Arial&quot;,&quot;sans-serif&quot;; font-size: 18.0pt;">en la más rabiosa actualidad </span></b></div><div align="center" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: center;"><b><span style="color: red; font-family: &quot;Arial&quot;,&quot;sans-serif&quot;; font-size: 18.0pt;">son por muchos habladas </span></b></div><div align="center" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: center;"><b><span style="color: red; font-family: &quot;Arial&quot;,&quot;sans-serif&quot;; font-size: 18.0pt;">como una rutina y por comodidad. </span></b></div><div align="center" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: center;"><br /></div><div align="center" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: center;"><b><span style="color: red; font-family: &quot;Arial&quot;,&quot;sans-serif&quot;; font-size: 18.0pt;">Se propagan cuan contagio y con repetición </span></b></div><div align="center" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: center;"><b><span style="color: red; font-family: &quot;Arial&quot;,&quot;sans-serif&quot;; font-size: 18.0pt;">y pululan en los medios de comunicación </span></b></div><div align="center" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: center;"><b><span style="color: red; font-family: &quot;Arial&quot;,&quot;sans-serif&quot;; font-size: 18.0pt;">en especial en la televisión.</span></b></div><div align="center" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: center;"><br /></div><div align="center" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: center;"><b><span style="color: red; font-family: &quot;Arial&quot;,&quot;sans-serif&quot;; font-size: 18.0pt;">Aquí tenemos la primera: </span></b></div><div align="center" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: center;"><b><span style="color: blue; font-family: &quot;Arial&quot;,&quot;sans-serif&quot;; font-size: 18.0pt;">No puede ser de otra manera </span></b></div><div align="center" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: center;"><b><span style="color: red; font-family: &quot;Arial&quot;,&quot;sans-serif&quot;; font-size: 18.0pt;">el entrevistado asevera </span></b></div><div align="center" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: center;"><b><span style="color: red; font-family: &quot;Arial&quot;,&quot;sans-serif&quot;; font-size: 18.0pt;">con una seguridad certera. </span></b></div><div align="center" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: center;"><br /></div><div align="center" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: center;"><b><span style="color: blue; font-family: &quot;Arial&quot;,&quot;sans-serif&quot;; font-size: 18.0pt;">La verdad es que sí </span></b></div><div align="center" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: center;"><b><span style="color: red; font-family: &quot;Arial&quot;,&quot;sans-serif&quot;; font-size: 18.0pt;">muchos? lo afirman con frenesí </span></b></div><div align="center" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: center;"><b><span style="color: red; font-family: &quot;Arial&quot;,&quot;sans-serif&quot;; font-size: 18.0pt;">&nbsp;y con bobalicona determinación </span></b></div><div align="center" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: center;"><b><span style="color: red; font-family: &quot;Arial&quot;,&quot;sans-serif&quot;; font-size: 18.0pt;">en improvisada contestación </span></b></div><div align="center" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: center;"><b><span style="color: red; font-family: &quot;Arial&quot;,&quot;sans-serif&quot;; font-size: 18.0pt;">al periodista de turno en acción. </span></b></div><div align="center" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: center;"><br /></div><div align="center" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: center;"><b><span style="color: red; font-family: &quot;Arial&quot;,&quot;sans-serif&quot;; font-size: 18.0pt;">En estos tiempos actuales </span></b></div><div align="center" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: center;"><b><span style="color: red; font-family: &quot;Arial&quot;,&quot;sans-serif&quot;; font-size: 18.0pt;">hay que considerar los movimientos sociales </span></b></div><div align="center" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: center;"><b><span style="color: red; font-family: &quot;Arial&quot;,&quot;sans-serif&quot;; font-size: 18.0pt;">en la ciudad cada vez hay mayor población </span></b></div><div align="center" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: center;"><b><span style="color: red; font-family: &quot;Arial&quot;,&quot;sans-serif&quot;; font-size: 18.0pt;">lógicamente viene la sustitución: </span></b></div><div align="center" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: center;"><b><span style="color: blue; font-family: &quot;Arial&quot;,&quot;sans-serif&quot;; font-size: 18.0pt;">ciudadanía</span></b><b><span style="color: red; font-family: &quot;Arial&quot;,&quot;sans-serif&quot;; font-size: 18.0pt;"> es la solución. </span></b></div><div align="center" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: center;"><br /></div><div align="center" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: center;"><b><span style="color: red; font-family: &quot;Arial&quot;,&quot;sans-serif&quot;; font-size: 18.0pt;">Y está organizada en distintas secciones </span></b></div><div align="center" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: center;"><b><span style="color: red; font-family: &quot;Arial&quot;,&quot;sans-serif&quot;; font-size: 18.0pt;">son los </span></b><b><span style="color: blue; font-family: &quot;Arial&quot;,&quot;sans-serif&quot;; font-size: 18.0pt;">colectivos</span></b><b><span style="color: red; font-family: &quot;Arial&quot;,&quot;sans-serif&quot;; font-size: 18.0pt;"> con sus diferentes denominaciones </span></b></div><div align="center" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: center;"><b><span style="color: red; font-family: &quot;Arial&quot;,&quot;sans-serif&quot;; font-size: 18.0pt;">los políticos conocen bien estas aglomeraciones </span></b></div><div align="center" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: center;"><b><span style="color: red; font-family: &quot;Arial&quot;,&quot;sans-serif&quot;; font-size: 18.0pt;">e intentan conseguir su voto en las elecciones. </span></b></div><div align="center" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: center;"><br /></div><div align="center" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: center;"><b><span style="color: blue; font-family: &quot;Arial&quot;,&quot;sans-serif&quot;; font-size: 18.0pt;">Discapacitado</span></b><b><span style="color: red; font-family: &quot;Arial&quot;,&quot;sans-serif&quot;; font-size: 18.0pt;"> es palabra de gran aceptación </span></b></div><div align="center" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: center;"><b><span style="color: red; font-family: &quot;Arial&quot;,&quot;sans-serif&quot;; font-size: 18.0pt;">versátil y polivalente </span></b></div><div align="center" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: center;"><b><span style="color: red; font-family: &quot;Arial&quot;,&quot;sans-serif&quot;; font-size: 18.0pt;">no suena mal y es muy procedente </span></b></div><div align="center" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: center;"><b><span style="color: red; font-family: &quot;Arial&quot;,&quot;sans-serif&quot;; font-size: 18.0pt;">para nombrar a distintos colectivos en cuestión. </span></b></div><div align="center" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: center;"><br /></div><div align="center" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: center;"><b><span style="color: red; font-family: &quot;Arial&quot;,&quot;sans-serif&quot;; font-size: 18.0pt;">Hay un vocablo de reciente irrupción </span></b></div><div align="center" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: center;"><b><span style="color: red; font-family: &quot;Arial&quot;,&quot;sans-serif&quot;; font-size: 18.0pt;">en su nueva acepción </span></b></div><div align="center" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: center;"><b><span style="color: red; font-family: &quot;Arial&quot;,&quot;sans-serif&quot;; font-size: 18.0pt;">y es de grande difusión. </span></b></div><div align="center" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: center;"><b><span style="color: red; font-family: &quot;Arial&quot;,&quot;sans-serif&quot;; font-size: 18.0pt;">Es verdad que tiene su antigüedad </span></b></div><div align="center" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: center;"><b><span style="color: red; font-family: &quot;Arial&quot;,&quot;sans-serif&quot;; font-size: 18.0pt;">en alguna que otra modalidad. </span></b></div><div align="center" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: center;"><br /></div><div align="center" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: center;"><b><span style="color: red; font-family: &quot;Arial&quot;,&quot;sans-serif&quot;; font-size: 18.0pt;">Me refiero a clásico, quizás lo habéis adivinado </span></b></div><div align="center" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: center;"><b><span style="color: red; font-family: &quot;Arial&quot;,&quot;sans-serif&quot;; font-size: 18.0pt;">un clásico (alusivo a clase) es digno de imitación </span></b></div><div align="center" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: center;"><b><span style="color: red; font-family: &quot;Arial&quot;,&quot;sans-serif&quot;; font-size: 18.0pt;">por lo tanto está muy valorado.</span></b></div><div align="center" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: center;"><br /></div><div align="center" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: center;"><b><span style="color: red; font-family: &quot;Arial&quot;,&quot;sans-serif&quot;; font-size: 18.0pt;">Clásico fue el arte griego y el romano con mucha influencia en el futuro.</span></b></div><div align="center" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: center;"><br /></div><div align="center" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: center;"><b><span style="color: red; font-family: &quot;Arial&quot;,&quot;sans-serif&quot;; font-size: 18.0pt;">Y en música, un periodo dieciochesco europeo de lenguaje equilibrado y maduro.</span></b></div><div align="center" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: center;"><br /></div><div align="center" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: center;"><b><span style="color: red; font-family: &quot;Arial&quot;,&quot;sans-serif&quot;; font-size: 18.0pt;">Pero? con el artículo cambiado</span></b></div><div align="center" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: center;"><b><span style="color: red; font-family: &quot;Arial&quot;,&quot;sans-serif&quot;; font-size: 18.0pt;">tiene un futbolístico significado:</span></b></div><div align="center" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: center;"><br /></div><div align="center" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: center;"><b><span style="color: blue; font-family: &quot;Arial&quot;,&quot;sans-serif&quot;; font-size: 18.0pt;">El clásico</span></b><b><span style="color: red; font-family: &quot;Arial&quot;,&quot;sans-serif&quot;; font-size: 18.0pt;"> es de gran trascendencia</span></b></div><div align="center" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: center;"><b><span style="color: red; font-family: &quot;Arial&quot;,&quot;sans-serif&quot;; font-size: 18.0pt;">y zumba en no pocas conciencias</span></b></div><div align="center" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: center;"><b><span style="color: red; font-family: &quot;Arial&quot;,&quot;sans-serif&quot;; font-size: 18.0pt;">provocando intensas emociones</span></b></div><div align="center" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: center;"><b><span style="color: red; font-family: &quot;Arial&quot;,&quot;sans-serif&quot;; font-size: 18.0pt;">por no decir irrefrenables pasiones...</span></b></div><div align="center" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: center;"><b><span style="color: red; font-family: &quot;Arial&quot;,&quot;sans-serif&quot;; font-size: 18.0pt;">Tal es su importancia,</span></b></div><div align="center" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: center;"><b><span style="color: red; font-family: &quot;Arial&quot;,&quot;sans-serif&quot;; font-size: 18.0pt;">su calado, su relevancia </span></b></div><div align="center" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: center;"><b><span style="color: red; font-family: &quot;Arial&quot;,&quot;sans-serif&quot;; font-size: 18.0pt;">que al poner la palabra "clásico" en el buscador,</span></b></div><div align="center" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: center;"><b><span style="color: red; font-family: &quot;Arial&quot;,&quot;sans-serif&quot;; font-size: 18.0pt;">me refiero al google del ordenador,</span></b></div><div align="center" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: center;"><b><span style="color: red; font-family: &quot;Arial&quot;,&quot;sans-serif&quot;; font-size: 18.0pt;">casi todas las paginas encontradas</span></b></div><div align="center" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: center;"><b><span style="color: red; font-family: &quot;Arial&quot;,&quot;sans-serif&quot;; font-size: 18.0pt;">indican la dichosa contienda liguera, casi adorada.</span></b></div><div align="center" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: center;"><br /></div><div align="center" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: center;"><b><span style="color: red; font-family: &quot;Arial&quot;,&quot;sans-serif&quot;; font-size: 18.0pt;">y para terminar esta versada descripción</span></b></div><div align="center" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: center;"><b><span style="color: red; font-family: &quot;Arial&quot;,&quot;sans-serif&quot;; font-size: 18.0pt;">me referiré a la palabra </span></b><b><span style="color: blue; font-family: &quot;Arial&quot;,&quot;sans-serif&quot;; font-size: 18.0pt;">operación</span></b></div><div align="center" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: center;"><b><span style="color: red; font-family: &quot;Arial&quot;,&quot;sans-serif&quot;; font-size: 18.0pt;">que tiene múltiples acepciones</span></b></div><div align="center" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: center;"><b><span style="color: red; font-family: &quot;Arial&quot;,&quot;sans-serif&quot;; font-size: 18.0pt;">una&nbsp; de ellas se refiere a las acciones</span></b></div><div align="center" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: center;"><b><span style="color: red; font-family: &quot;Arial&quot;,&quot;sans-serif&quot;; font-size: 18.0pt;">de políticos, maleantes y ladrones:</span></b></div><div align="center" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: center;"><br /></div><div align="center" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: center;"><b><span style="color: red; font-family: &quot;Arial&quot;,&quot;sans-serif&quot;; font-size: 18.0pt;">Después de la operación</span></b></div><div align="center" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: center;"><b><span style="color: red; font-family: &quot;Arial&quot;,&quot;sans-serif&quot;; font-size: 18.0pt;">de la necesaria recalificación</span></b></div><div align="center" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: center;"><b><span style="color: blue; font-family: &quot;Arial&quot;,&quot;sans-serif&quot;; font-size: 18.0pt;">para nada</span></b><b><span style="color: red; font-family: &quot;Arial&quot;,&quot;sans-serif&quot;; font-size: 18.0pt;"> ha sido </span></b><b><span style="color: blue; font-family: &quot;Arial&quot;,&quot;sans-serif&quot;; font-size: 18.0pt;">complicado</span></b><b><span style="color: red; font-family: &quot;Arial&quot;,&quot;sans-serif&quot;; font-size: 18.0pt;">:</span></b><b><span style="color: blue; font-family: &quot;Arial&quot;,&quot;sans-serif&quot;; font-size: 18.0pt;">&nbsp;</span></b><b><span style="color: red; font-family: &quot;Arial&quot;,&quot;sans-serif&quot;; font-size: 18.0pt;"> </span></b></div><div align="center" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: center;"><b><span style="color: red; font-family: &quot;Arial&quot;,&quot;sans-serif&quot;; font-size: 18.0pt;">De ser </span></b><b><span style="color: blue; font-family: &quot;Arial&quot;,&quot;sans-serif&quot;; font-size: 18.0pt;">imputado</span></b></div><div align="center" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: center;"><b><span style="color: red; font-family: &quot;Arial&quot;,&quot;sans-serif&quot;; font-size: 18.0pt;">me he librado</span></b></div><div align="center" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: center;"><b><span style="color: red; font-family: &quot;Arial&quot;,&quot;sans-serif&quot;; font-size: 18.0pt;">ya que soy aforado</span></b></div><div align="center" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: center;"><b><span style="color: red; font-family: &quot;Arial&quot;,&quot;sans-serif&quot;; font-size: 18.0pt;">y lo tengo todo controlado.&nbsp;</span></b></div><div align="center" class="MsoNormal" style="margin-bottom: .0001pt; margin-bottom: 0cm; text-align: center;"><br /></div><br /><br /></div><div style="text-align: center;"><!--[if gte mso 9]><xml> <w:WordDocument>  <w:View>Normal</w:View>  <w:Zoom>0</w:Zoom>  <w:TrackMoves/>  <w:TrackFormatting/>  <w:HyphenationZone>21</w:HyphenationZone>  <w:PunctuationKerning/>  <w:ValidateAgainstSchemas/>  <w:SaveIfXMLInvalid>false</w:SaveIfXMLInvalid>  <w:IgnoreMixedContent>false</w:IgnoreMixedContent>  <w:AlwaysShowPlaceholderText>false</w:AlwaysShowPlaceholderText>  <w:DoNotPromoteQF/>  <w:LidThemeOther>ES</w:LidThemeOther>  <w:LidThemeAsian>X-NONE</w:LidThemeAsian>  <w:LidThemeComplexScript>X-NONE</w:LidThemeComplexScript>  <w:Compatibility>   <w:BreakWrappedTables/>   <w:SnapToGridInCell/>   <w:WrapTextWithPunct/>   <w:UseAsianBreakRules/>   <w:DontGrowAutofit/>   <w:SplitPgBreakAndParaMark/>   <w:DontVertAlignCellWithSp/>   <w:DontBreakConstrainedForcedTables/>   <w:DontVertAlignInTxbx/>   <w:Word11KerningPairs/>   <w:CachedColBalance/>  </w:Compatibility>  <m:mathPr>   <m:mathFont m:val="Cambria Math"/>   <m:brkBin m:val="before"/>   <m:brkBinSub m:val="--"/>   <m:smallFrac m:val="off"/>   <m:dispDef/>   <m:lMargin m:val="0"/>   <m:rMargin m:val="0"/>   <m:defJc m:val="centerGroup"/>   <m:wrapIndent m:val="1440"/>   <m:intLim m:val="subSup"/>   <m:naryLim m:val="undOvr"/>  </m:mathPr></w:WordDocument></xml><![endif]--></div><div style="text-align: center;"><!--[if gte mso 9]><xml> <w:LatentStyles DefLockedState="false" DefUnhideWhenUsed="true"   DefSemiHidden="true" DefQFormat="false" DefPriority="99"  