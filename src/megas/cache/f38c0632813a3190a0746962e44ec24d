se"    UnhideWhenUsed="false" Name="Colorful List Accent 5"/>  <w:LsdException Locked="false" Priority="73" SemiHidden="false"    UnhideWhenUsed="false" Name="Colorful Grid Accent 5"/>  <w:LsdException Locked="false" Priority="60" SemiHidden="false"    UnhideWhenUsed="false" Name="Light Shading Accent 6"/>  <w:LsdException Locked="false" Priority="61" SemiHidden="false"    UnhideWhenUsed="false" Name="Light List Accent 6"/>  <w:LsdException Locked="false" Priority="62" SemiHidden="false"    UnhideWhenUsed="false" Name="Light Grid Accent 6"/>  <w:LsdException Locked="false" Priority="63" SemiHidden="false"    UnhideWhenUsed="false" Name="Medium Shading 1 Accent 6"/>  <w:LsdException Locked="false" Priority="64" SemiHidden="false"    UnhideWhenUsed="false" Name="Medium Shading 2 Accent 6"/>  <w:LsdException Locked="false" Priority="65" SemiHidden="false"    UnhideWhenUsed="false" Name="Medium List 1 Accent 6"/>  <w:LsdException Locked="false" Priority="66" SemiHidden="false"    UnhideWhenUsed="false" Name="Medium List 2 Accent 6"/>  <w:LsdException Locked="false" Priority="67" SemiHidden="false"    UnhideWhenUsed="false" Name="Medium Grid 1 Accent 6"/>  <w:LsdException Locked="false" Priority="68" SemiHidden="false"    UnhideWhenUsed="false" Name="Medium Grid 2 Accent 6"/>  <w:LsdException Locked="false" Priority="69" SemiHidden="false"    UnhideWhenUsed="false" Name="Medium Grid 3 Accent 6"/>  <w:LsdException Locked="false" Priority="70" SemiHidden="false"    UnhideWhenUsed="false" Name="Dark List Accent 6"/>  <w:LsdException Locked="false" Priority="71" SemiHidden="false"    UnhideWhenUsed="false" Name="Colorful Shading Accent 6"/>  <w:LsdException Locked="false" Priority="72" SemiHidden="false"    UnhideWhenUsed="false" Name="Colorful List Accent 6"/>  <w:LsdException Locked="false" Priority="73" SemiHidden="false"    UnhideWhenUsed="false" Name="Colorful Grid Accent 6"/>  <w:LsdException Locked="false" Priority="19" SemiHidden="false"    UnhideWhenUsed="false" QFormat="true" Name="Subtle Emphasis"/>  <w:LsdException Locked="false" Priority="21" SemiHidden="false"    UnhideWhenUsed="false" QFormat="true" Name="Intense Emphasis"/>  <w:LsdException Locked="false" Priority="31" SemiHidden="false"    UnhideWhenUsed="false" QFormat="true" Name="Subtle Reference"/>  <w:LsdException Locked="false" Priority="32" SemiHidden="false"    UnhideWhenUsed="false" QFormat="true" Name="Intense Reference"/>  <w:LsdException Locked="false" Priority="33" SemiHidden="false"    UnhideWhenUsed="false" QFormat="true" Name="Book Title"/>  <w:LsdException Locked="false" Priority="37" Name="Bibliography"/>  <w:LsdException Locked="false" Priority="39" QFormat="true" Name="TOC Heading"/> </w:LatentStyles></xml><![endif]--><!--[if gte mso 10]><style> /* Style Definitions */  table.MsoNormalTable  {mso-style-name:"Tabla normal";  mso-tstyle-rowband-size:0;  mso-tstyle-colband-size:0;  mso-style-noshow:yes;  mso-style-priority:99;  mso-style-qformat:yes;  mso-style-parent:"";  mso-padding-alt:0cm 5.4pt 0cm 5.4pt;  mso-para-margin:0cm;  mso-para-margin-bottom:.0001pt;  line-height:115%;  mso-pagination:widow-orphan;  font-size:11.0pt;  font-family:"Calibri","sans-serif";  mso-ascii-font-family:Calibri;  mso-ascii-theme-font:minor-latin;  mso-hansi-font-family:Calibri;  mso-hansi-theme-font:minor-latin;  mso-fareast-language:EN-US;} </style><![endif]--><!--[if gte mso 9]><xml> <o:shapedefaults v:ext="edit" spidmax="1026"/></xml><![endif]--><!--[if gte mso 9]><xml> <o:shapelayout v:ext="edit">  <o:idmap v:ext="edit" data="1"/> </o:shapelayout></xml><![endif]--> </div><div align="center" class="MsoNormal" style="text-align: center;"><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;">&nbsp;Son conocidos por las siglas Oopart (Out of place artifact = Artefacto fuera de lugar) aunque el nombre del título es más correcto, pues no todos los objetos son artefactos y el factor determinante es el tiempo, no el lugar.</span></b></span></span></div><div align="center" class="MsoNormal" style="text-align: center;"><br /></div><div align="center" class="MsoNormal" style="text-align: center;"><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;">Hay muchos de estos objetos que se han encontrado ?fosilizados" en estratos rocosos de muy diversas épocas y que no encajan con la época del estrato y por lo tanto del objeto.</span></b></span></span></div><div align="center" class="MsoNormal" style="text-align: center;"><br /></div><div align="center" class="MsoNormal" style="text-align: center;"><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;">Según la wikipedia un oopart es un objeto de interés histórico, arqueológico o paleontológico que se encuentra en un contexto muy inusual o aparentemente imposible que podría desafiar la cronología de la historia convencional.</span></b></span></span></div><div align="center" class="MsoNormal" style="text-align: center;"><br /></div><div align="center" class="MsoNormal" style="text-align: center;"><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;">Su ubicación cronológica puede situarse desde el precámbrico hasta épocas históricas de la humanidad pero casi siempre muestran señales de adelantos tecnológicos imposibles para la época en que se han encontrado.</span></b></span></span></div><div align="center" class="MsoNormal" style="text-align: center;"><br /></div><div align="center" class="MsoNormal" style="text-align: center;"><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;">Para los científicos ortodoxos, tales objetos o bien son falsificaciones o bien no tienen respuesta y los ignoran o bien son interpretaciones erróneas.</span></b></span></span></div><div align="center" class="MsoNormal" style="text-align: center;"><br /></div><div align="center" class="MsoNormal" style="text-align: center;"><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;">Para otros investigadores que se los toman más en serio sacan conclusiones más inquietantes, que echan por tierra el origen y la historia oficial de la humanidad:</span></b></span></span></div><div align="center" class="MsoNormal" style="text-align: center;"><br /></div><div align="center" class="MsoListParagraphCxSpFirst" style="text-align: center; text-indent: -18pt;"><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;">1.<span style="-moz-font-feature-settings: normal; -moz-font-language-override: normal; font-size-adjust: none; font-stretch: normal; font-style: normal; font-variant: normal; font-weight: normal; line-height: normal;">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; </span>La especie humana es mucho más antigua de lo que nos han enseñado y su nivel científico y tecnológico también.</span></b></span></span></div><div align="center" class="MsoListParagraphCxSpFirst" style="text-align: center; text-indent: -18pt;"><br /></div><div align="center" class="MsoListParagraphCxSpMiddle" style="text-align: center; text-indent: -18pt;"><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;">2.<span style="-moz-font-feature-settings: normal; -moz-font-language-override: normal; font-size-adjust: none; font-stretch: normal; font-style: normal; font-variant: normal; font-weight: normal; line-height: normal;">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; </span>Es posible que hayan existido otras humanidades, ya incluso en el precámbrico y que han sido destruidas por causas naturales o artificiales (autodestrucción, exterminio por parte de ?dioses??).</span></b></span></span></div><div align="center" class="MsoListParagraphCxSpMiddle" style="text-align: center; text-indent: -18pt;"><br /></div><div align="center" class="MsoListParagraphCxSpLast" style="text-align: center; text-indent: -18pt;"><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;">3.<span style="-moz-font-feature-settings: normal; -moz-font-language-override: normal; font-size-adjust: none; font-stretch: normal; font-style: normal; font-variant: normal; font-weight: normal; line-height: normal;">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; </span>Estos objetos fuera de tiempo pudieron ser hechos por seres humanoides extraterrestres (dioses) que desde el origen de nuestro planeta han deambulado por él.</span></b></span></span></div><div align="center" class="MsoListParagraphCxSpLast" style="text-align: center; text-indent: -18pt;"><br /></div><div align="center" class="MsoListParagraphCxSpLast" style="text-align: center; text-indent: -18pt;"><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;">4. &nbsp; Han sido efectuados por viajeros en el tiempo, probablemente desde el futuro al pasado. </span></b></span></span></div><div align="center" class="MsoNormal" style="text-align: center;"><br /></div><div align="center" class="MsoNormal" style="text-align: center;"><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;">Son algunas opciones que merecen ser tenidas en cuenta e investigadas.</span></b></span></span></div><div align="center" class="MsoNormal" style="text-align: center;"><br /></div><div align="center" class="MsoNormal" style="text-align: center;"><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;">A continuación enlazo cinco páginas y dos videos que detallan este fascinante tema y que dan ejemplos de los principales ooparts.</span></b></span></span></div><div align="center" class="MsoNormal" style="text-align: center;"><br /></div><div align="center" class="MsoNormal" style="text-align: center;"><a href="http://es.wikipedia.org/wiki/Oopart"><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;"><span style="color: orange;">Página de wikipedia</span></span></b></span></span></a></div><div align="center" class="MsoNormal" style="text-align: center;"><br /></div><div align="center" class="MsoNormal" style="text-align: center;"><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;"><span style="color: orange;"><a href="http://esencia21.wordpress.com/2009/09/25/los-10-objetos-fuera-del-tiempo-mas-extrano/"><span style="color: orange;">Página 2</span> </a></span></span></b></span></span></div><div align="center" class="MsoNormal" style="text-align: center;"><br /></div><div align="center" class="MsoNormal" style="text-align: center;"><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;"><span style="color: orange;"><a href="http://proyectoverdad.wordpress.com/2013/06/03/los-ooparts-objetos-fuera-del-tiempo/"><span style="color: orange;">Página 3</span></a>&nbsp; </span></span></b></span></span></div><div align="center" class="MsoNormal" style="text-align: center;"><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;"><span style="color: orange;"><br /></span></span></b></span></span></div><div align="center" class="MsoNormal" style="text-align: center;"><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;"><span style="color: orange;"><a href="http://listas.20minutos.es/lista/15-cosas-fuera-de-sitio-oopart-52658/"><span style="color: orange;">Página 4</span></a></span></span></b></span></span><br /><br /><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;"><span style="color: orange;"><span style="color: orange;"><span style="color: orange;"><a href="http://despiertacordoba.wordpress.com/2014/11/01/pisadas-de-los-viajeros-del-tiempo/#more-6524">Página 5</a></span> </span></span></span></b></span></span></div><div align="center" class="MsoNormal" style="text-align: center;"><br /></div><div align="center" class="MsoNormal" style="text-align: center;"><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;"><span style="color: orange;"><span style="color: orange;"><a href="https://www.youtube.com/watch?v=5gtfEPk173U&amp;list=UUmdvAxEJ14EvXdASKbodj1Q">Vídeo 1</a>&nbsp;</span></span></span></b></span></span></div><div align="center" class="MsoNormal" style="text-align: center;"><br /></div><div align="center" class="MsoNormal" style="text-align: center;"><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;"><span style="color: orange;"><span style="color: orange;"><a href="http://granmisterio.org/2014/10/19/las-esferas-del-fin-del-mundo-el-oopart-de-klerksdorp/">Vídeo 2</a>&nbsp;</span> </span></span></b></span></span></div><div align="center" class="MsoNormal" style="text-align: center;"><br /></div><div align="center" class="MsoNormal" style="text-align: center;"></div><div class="separator" style="clear: both; text-align: center;"><a href="http://4.bp.blogspot.com/-DBOBoQ7kZuI/VFVjA2QQVRI/AAAAAAAACzo/KG5oPsqfL7U/s1600/ooparts_teschio.jpg" imageanchor="1" style="margin-left: 1em; margin-right: 1em;"><img border="0" src="http://4.bp.blogspot.com/-DBOBoQ7kZuI/VFVjA2QQVRI/AAAAAAAACzo/KG5oPsqfL7U/s1600/ooparts_teschio.jpg" /></a></div><div align="center" class="MsoNormal" style="text-align: center;"><br /></div><div align="center" class="MsoNormal" style="text-align: center;"><br /></div><div align="center" class="MsoNormal" style="text-align: center;"><br /></div><div align="center" class="MsoNormal" style="text-align: center;"><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;"><span style="color: orange;">&nbsp;</span> </span></b></span></span></div>";s:12:"link_replies";s:153:"http://realyfantastico.blogspot.com/feeds/1957598579909176012/comments/defaulthttp://www.blogger.com/comment.g?blogID=35097098&postID=1957598579909176012";s:9:"link_edit";s:71:"http://www.blogger.com/feeds/35097098/posts/default/1957598579909176012";s:9:"link_self";s:71:"http://www.blogger.com/feeds/35097098/posts/default/1957598579909176012";s:4:"link";s:72:"http://realyfantastico.blogspot.com/2014/11/objetos-fuera-de-tiempo.html";s:11:"author_name";s:41:"musicoclasico parapoeta real y fantástico";s:10:"author_uri";s:51:"http://www.blogger.com/profile/00422908465900985272";s:12:"author_email";s:19:"noreply@blogger.com";}i:17;a:12:{s:2:"id";s:59:"tag:blogger.com,1999:blog-35097098.post-5748570701679327404";s:9:"published";s:29:"2014-10-16T11:36:00.003+02:00";s:7:"updated";s:29:"2014-10-17T15:14:41.760+02:00";s:5:"title";s:46:"¿Es nuestro ADN de procedencia extraterrestre?";s:12:"atom_content";s:30568:"<div style="text-align: center;"><span style="font-family: &quot;Helvetica Neue&quot;,Arial,Helvetica,sans-serif;"><span style="font-size: large;"><b><span style="color: red;"><br /></span></b></span></spa